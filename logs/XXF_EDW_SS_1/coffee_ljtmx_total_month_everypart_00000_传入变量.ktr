  <step>
    <name>sisyphe_coff_sale_month_score表输入</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>coff</connection>
    <sql>SELECT final_year,final_year_month,station_code,
			 (case when revenue_rate_score is null then 0 else revenue_rate_score end ) revenue_rate_score,
        (case when purchase_rate_score is null then 0 else purchase_rate_score end ) purchase_rate_score,
        (case when card_revenue_rate_score is null then 0 else card_revenue_rate_score end )card_revenue_rate_score,
        (case when per_order_money0_score is null then 0 else per_order_money0_score end )per_order_money0_score,
        (case when seating_transactions0_rate_score is null then 0 else seating_transactions0_rate_score end )seating_transactions0_rate_score,
			  (case when per_order_amount0_score is null then 0 else per_order_amount0_score end )per_order_amount0_score,
        (case when total_socre is null then 0 else total_socre end )total_socre
from
(SELECT t0.year final_year,t0.final_year_month,t0.station_code,revenue_rate_score,purchase_rate_score,card_revenue_rate_score,per_order_money0_score,seating_transactions0_rate_score,
per_order_amount0_score,
revenue_rate_score*0.3+purchase_rate_score*0.2+card_revenue_rate_score*0.15+per_order_money0_score*0.15+seating_transactions0_rate_score*0.1+
per_order_amount0_score*0.1 as total_socre
from
	(select station_code,y as year,final_year_month from (select station_code from sisyphe_coff.sisyphe_coff_station_base_info where station_code&lt;&gt;'HDQA00')t1,
(SELECT DISTINCT y,final_year_month from sisyphe_dds.time_dim_fina_date where  final_year_month='${MAX_YEARMONTH}') t2) t0
	  left join
(select final_year_month,station_code,
case when revenue_rate&lt;0.9 then 0 else (case when revenue_rate>=0.97 then 100 else 60 end) end as revenue_rate_score,
case when purchase_rate&lt;0.85 then 0 else (case when purchase_rate>=0.95 then 100 else 60 end) end as purchase_rate_score,
case when card_revenue_rate&lt;0.5 then 0 else (case when card_revenue_rate>=0.7 then 100 else 60 end) end as card_revenue_rate_score,
case when per_order_money0&lt;48 then 0 else (case when per_order_money0>=55 then 100 else 60 end) end as  per_order_money0_score,
case when seating_transactions0_rate&lt;0.5 then 0 else (case when seating_transactions0_rate>=1 then 100 else 60 end) end as  seating_transactions0_rate_score,
case when per_order_amount0&lt;1.2 then 0 else (case when per_order_amount0>=1.8 then 100 else 60 end) end as  per_order_amount0_score
from sisyphe_coff.sisyphe_coff_sale_index_month_temp
  where final_year_month='${MAX_YEARMONTH}') t
on t0.final_year_month=t.final_year_month and t0.station_code=t.station_code) temp;</sql>
