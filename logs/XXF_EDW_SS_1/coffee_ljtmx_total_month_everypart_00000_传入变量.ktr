    <connection>coff</connection>
    <sql>select  left(t1.final_year_month,4) final_year,t1.final_year_month,t1.station_code,
t1.score as lost_socre, t2.score as stock_erro_socre, t3.standard_difference_score,t4.score as maintain_compare_score,t5.score as equipment_repair_score,
t1.score*0.2+t2.score*0.1+t3.standard_difference_score*0.2+t4.score*0.3+t5.score*0.2 as total_score
FROM (select final_year_month,station_code,score from sisyphe_coff.sisyphe_coff_equipment_appliances_lost_money_month where final_year_month='${MAX_YEARMONTH}') t1
inner join sisyphe_coff.sisyphe_coff_equipment_appliances_stock_erro_month t2
on t1.final_year_month=t2.final_year_month and t1.station_code=t2.station_code
inner join sisyphe_coff.sisyphe_coff_equipment_appliances_standard_month t3
on t1.final_year_month=t3.final_year_month and t1.station_code=t3.station_code
inner join sisyphe_coff.sisyphe_coff_equipment_maintain_compare_month t4
on t1.final_year_month=t4.final_year_month and t1.station_code=t4.station_code
inner join sisyphe_coff.sisyphe_coff_equipment_repair_month t5
on t1.final_year_month=t5.final_year_month and t1.station_code=t5.station_code;</sql>
