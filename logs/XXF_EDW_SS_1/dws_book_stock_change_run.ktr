    <sql>/*
每天的变化量
 */
select from_location_code,
       库位名称                            as storage_name,
       date(create_time)               as create_time,
       fina_category_name,
       category_two_name,
       category_three_name,
       category_four_name,
       category_five_name,
       product_isbn,
       product_name,
       product_code,
       product_price,
       sum(a.数量 * a.产品码洋 * a.状态)       as change_money,-- 变化金额
       sum(a.数量 * a.状态)                as change_amount,-- 变化数量
       sum(a.数量 * a.flow_price * a.状态) as price, -- 流转成本
       sum(a.数量 * a.cost_price * a.状态) as cost_price -- 进货成本
from (select a.station_code as from_location_code,
             d.category_two_name,
             d.category_three_name,
             d.category_four_name,
             d.category_five_name,
             d.product_isbn,
             d.product_price,
             b.操作类型名称       as 库位名称,
             a.create_time,
             d.fina_category_name,
             d.product_name,
             d.product_code,
             a.amount       as 数量,
             a.price        as 产品码洋,
             a.flow_price,
             a.cost_price,
             -in_out_flag   as 状态
      from cn_sisyphe_dwd.stock_change_detail_daily a
               left join cn_sisyphe_dds.操作类型_view as b on a.storage = b.操作类型编码 and a.date = '${a}'
               inner join cn_sisyphe_dds.product as d
                          on a.product_code = d.product_code and a.date = '${a}'
     ) a
group by from_location_code, 库位名称, date(create_time), fina_category_name, category_two_name, product_name, product_code,
         category_three_name, category_four_name, category_five_name, product_isbn, product_price;
</sql>
