    <connection>192.168.0.2_xxfbooks</connection>
    <sql>select '书店' as 类别,CONVERT(varchar(10),stock_date,21) as 支付日期,b.station_name as 站点,a.station_id as 站点ID,sum(a.total_money) as 批销码洋,sum(a.real_money) as 批销实洋
from
yw_px a
inner join
db_station_total b
on a.station_id=b.station_id
where CONVERT(varchar(10),stock_date,21)>'2018-12-31'
