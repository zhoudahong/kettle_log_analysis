    <sql>SELECT
  convert(varchar(50),db_ls.ls_datetime,23) as 操作时间,
left(db_product.h_type,2) as 商品一级分类,
	class_id = db_pos_muli_pay.pos_pay_id,db_station_total.station_name as 站点名称,db_station_total.station_id as 站点编码,sum(db_ls.total_amount) as 销售册数,
	实洋 = sum(
		CASE
		WHEN real_money = 0 THEN
			0
		ELSE
			pos_charge * h_discount * h_amount * h_price / NULLIF(real_money,0)
		END
	),
	码洋 = sum(
		CASE
		WHEN real_money = 0 THEN
			0
		ELSE
			h_amount * h_price * pos_charge / NULLIF(dbo.uf_GetPosCharge (db_ls.ls_id),0)
		END
	),
	成本 = sum(
		CASE
		WHEN real_money = 0 THEN
			0
		ELSE
			crush_money * pos_charge / NULLIF(dbo.uf_GetPosCharge (db_ls.ls_id),0)
		END
	),
	支付方式 = (
		SELECT
			dz_paymethods.p_name
		FROM
			dz_paymethods
		WHERE
			db_pos_muli_pay.pos_pay_id = dz_paymethods.p_id
	)
FROM
	db_ls,
	db_pos_muli_pay,
	db_ls_item,
	db_product,
  db_station_total,
     dz_type
WHERE
	(
		db_ls_item.ls_id = db_ls.ls_id
	)
AND (
	db_pos_muli_pay.pos_id = db_ls.ls_id
)
AND db_product.h_id = db_ls_item.h_id
AND (db_ls.station_id=db_station_total.station_id)
and db_product.h_type=dz_type.h_type
AND convert(varchar(50),db_ls.ls_datetime,23)>convert(varchar(50),getdate()-9,23)
/*
 AND (
	(
		  (
			(
				db_ls.ls_datetime BETWEEN CONVERT (
					datetime,
					'2019-01-18 00:00:00.000'
				)
				AND CONVERT (
					datetime,
					'2019-01-18 23:59:59.998'
				) and station_name='北京昌发展万科广场店'
			)
		)
	)
)*/
GROUP BY left(db_product.h_type,2),
	db_pos_muli_pay.pos_pay_id,station_name,convert(varchar(50),db_ls.ls_datetime,23),db_station_total.station_id
