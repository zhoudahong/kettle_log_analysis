    <connection>coff</connection>
    <sql>select y as final_year,w,min(b.date) as date_min,max(b.date) as date_max,station_code,
sum(a.total_transactions-a.takeout_transactions) as total_amount0,
sum(a.seating_count) as seating_count,
sum(a.total_transactions-a.takeout_transactions)/sum(a.seating_count) as seating_transactions0_rate
from
(
select create_date,station_code,total_transactions,takeout_transactions,seating_count from sisyphe_coff.sisyphe_coff_sale_log
where create_date 
BETWEEN 
      (SELECT min(date) from sisyphe_dds.time_dim_fina_date where w in (select w from sisyphe_dds.time_dim_fina_date where date='${W_MAX_DATE}') and y=year('${W_MAX_DATE}'))
      and
      (SELECT max(date) from sisyphe_dds.time_dim_fina_date where w in (select w from sisyphe_dds.time_dim_fina_date where date='${W_MAX_DATE}') and y=year('${W_MAX_DATE}'))
) a 
inner join sisyphe_dds.time_dim_fina_date b 
on a.create_date=b.date
 GROUP BY w,station_code;</sql>
