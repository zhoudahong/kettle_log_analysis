  <step>
    <name>表输入sisyphe_coff_cargo_onlineinventory</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>coff</connection>
    <sql>select y,w,station_code,material_code,material_name,inventory_amount,usage_amount,cargo_unit,memo
from(
select b.w,b.y,a.station_code,c.material_code,c.material_name,
sum(a.dailyinventory_amount) as inventory_amount ,sum(a.dailyusage_amount) as usage_amount,c.cargo_unit,
case when sum(a.dailyusage_amount) is null then '无耗用' else '有耗用 'end as memo
from (
select stock_date,station_code,cargo_code,dailyinventory_amount,dailyusage_amount
from sisyphe_coff.`sisyphe_coff_cargo_onlineinventory` 
where cargo_status&lt;&gt;'过期' 
and cargo_code not in (select DISTINCT cargo_code from sisyphe_coff.sisyphe_coff_cargo_exclude 
												where up_date in (select DISTINCT max(up_date) as up_date from sisyphe_coff.sisyphe_coff_cargo_exclude ))
 ) a
INNER JOIN (
						select date,y,w from sisyphe_dds.time_dim_fina_date where w in 
										(SELECT w from sisyphe_dds.time_dim_fina_date where date='${W_MAX_DATE}'
)
and y=year('${W_MAX_DATE}')
) b
on a.stock_date=b.date
INNER JOIN (SELECT DISTINCT cargo_code,material_code,material_name,cargo_unit FROM sisyphe_dds.`material_cargo_table`) c
on a.cargo_code=c.cargo_code
group by b.w,b.y,a.station_code,c.material_code,c.material_name
)T
where memo='无耗用' </sql>
