  <step>
    <name>供应商库存</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>192.168.21.44_cn_sisyphe_ods</connection>
    <sql>select create_time,                                                                                                                 -- 每天的自然日
       category_two_name,                                                                                                           -- as 一级分类,
       supplier_code,-- 供应商编码
       supplier_name,                                                                                                               -- as 供应商名称,
       product_state,                                                                                                               -- as 新老品,
       logistic_code,                                                                                                               -- 物流编码
       logistic_name,                                                                                                               -- 物流名称
       stock_amount,                                                                                                                -- as 库存数量,
       stock_total_price,                                                                                                           -- as 库存码洋,
       stock_real_orice,                                                                                                            -- as 库存成本
       count(a.goods_code)goods_code_amount
from (select DATE_SUB(curdate(), INTERVAL 1 DAY)       create_time,                                                                               -- 每天的自然日
             c.category_two_name,                                                                                                                 -- as 一级分类,
             c.supplier_code,-- 供应商编码
             c.supplier_name,                                                                                                                     -- as 供应商名称,
             c.product_state,                                                                                                                     -- as 新老品,
             b.物流编码                                    logistic_code,                                                                             -- 物流编码
             b.物流名称                                    logistic_name,                                                                             -- 物流名称
             sum(a.available_amount)                   stock_amount,                                                                              -- as 库存数量,
             sum(a.available_amount) * c.product_price stock_total_price,                                                                         -- as 库存码洋,
             sum(a.available_amount) * e.cost_price    stock_real_orice,                                                                          -- as 库存成本
             a.goods_code -- as 商品编码
      from cn_sisyphe_dwd.dwd_book_offset_stock_all a
             inner join cn_sisyphe_dds.dds_station_view as b
             inner join cn_sisyphe_dds.product as c on a.goods_code = c.product_code
             inner join cn_sisyphe_dds.dds_station as d on a.location_code = d.new_station_code
             inner join cn_sisyphe_dwd.dwd_book_offset_batch_inc as e
               on a.batch_no = e.batch_no and a.goods_code = e.goods_code
      where a.location_code = b.新系统站点ID
      group by c.category_two_name,
               c.supplier_code,
               c.supplier_name,
               c.product_state,
               b.物流编码,
               b.物流名称)a where a.create_time>?
group by create_time,
         category_two_name,
         supplier_code,
         supplier_name,
         product_state,
         logistic_code,
         logistic_name,
         stock_amount,
         stock_total_price,
         stock_real_orice</sql>
