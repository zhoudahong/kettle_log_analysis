    <connection>192.168.21.44_cn_sisyphe_dwd</connection>
    <sql>select cast('${a}' as char) as date,
       a.batch_no,
       c.supplier_code,
       a.goods_code         as product_code,
       a.location_code      as station_code,
       a.storage,
       a.amount,
       b.price,
       b.flow_price,
       c.cost_price
from (
         select batch_no, goods_code, location_code, storage, sum(amount) as amount
         from (
                  select batch_no, goods_code, location_code, storage, available_amount as amount
                  from cn_sisyphe_ods.test_offset_stock
                  where available_amount > 0
                  union all
                  select batch_no,
                         goods_code,
                         from_location_code,
                         from_location_storage,
                         amount * case bill_purpose
                                      when 'OUT_STORAGE' then 1
                                      when 'IN_STORAGE' then -1
                                      else 0 end as amount
                  from cn_sisyphe_ods.test_offset_offset
                  where create_time between date_add('${a}', interval 1 day) and curdate()
              ) a
         group by batch_no, goods_code, location_code, storage
         having sum(amount) &lt;&gt; 0
         order by null) a
         inner join cn_sisyphe_ods.test_offset_stock b
                    on a.batch_no = b.batch_no and a.goods_code = b.goods_code and a.location_code = b.location_code and
                       a.storage = b.storage
         inner join cn_sisyphe_ods.test_offset_batch c
                    on a.batch_no = c.batch_no and a.goods_code = c.goods_code;
</sql>
