  <step>
    <name>储值卡新老卡</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>192.168.21.44_sisyphe_ods</connection>
    <sql>select date,
       station_code,
       operator_code,
       card_number,
       sum(recharge_realmoney)  as recharge_realmoney,
       sum(recharge_totalmoney) as recharge_totalmoney,
       sum(recharge_count)      as recharge_count,
       sum(consume_realmoney)   as consume_realmoney,
       sum(consume_totalmoney)  as consume_totalmoney,
       sum(consume_count)       as consume_count
from (
-- 储值卡充值
         select a.date,
                a.station_code,
                a.operator_code,
                a.card_number,
                sum(a.recharge_realmoney)                  as recharge_realmoney,
                sum(a.recharge_totalmoney)                  as recharge_totalmoney,
                sum(a.recharge_count) - sum(a.recharge_back_count) as recharge_count,
                0                            as consume_realmoney,
                0                            as consume_totalmoney,
                0                            as consume_count
         from (
                  select station_code,
                         operator_code,
                         date_format(create_time, '%Y-%m-%d') as date,
                         card_number,
                         sum(in_out_flag * pay_amount)        as recharge_realmoney,
                         sum(in_out_flag * trade_amount)      as recharge_totalmoney,
                         0                                    as recharge_count,
                         0                                    as recharge_back_count
                  from cn_sisyphe_ods.askcard_db_trade_record
                  where trade_type in
                        ('Recharge', 'RechargeAdjustAdd', 'RechargeAdjustReduce', 'RechargeRefund', 'RechargeRollback')
                    and status = 'Succeed'
                    and date_format(create_time, '%Y-%m-%d') = '${a}'
                  group by station_code,
                           operator_code,
                           date_format(create_time, '%Y-%m-%d'),
                           card_number
                  union all
                  select station_code,
                         operator_code,
                         date_format(create_time, '%Y-%m-%d') as date,
                         card_number,
                         0                                    as recharge_realmoney,
                         0                                    as recharge_totalmoney,
                         count(trade_number)                  as recharge_count,
                         0                                    as recharge_back_count
                  from cn_sisyphe_ods.askcard_db_trade_record
                  where trade_type = 'Recharge'
                    and status = 'Succeed'
                    and date_format(create_time, '%Y-%m-%d') = '${a}'
                  group by station_code,
                           operator_code,
                           date_format(create_time, '%Y-%m-%d'),
                           card_number
                  union all
                  select a.station_code,
                         a.operator_code,
                         date_format(a.create_time, '%Y-%m-%d') as date,
                         a.card_number,
                         0                                      as recharge_realmoney,
                         0                                      as recharge_totalmoney,
                         0                                      as recharge_count,
                         count(a.trade_number)                  as recharge_back_count
                  from cn_sisyphe_ods.askcard_db_trade_record a
                           inner join cn_sisyphe_ods.askcard_db_trade_record b on a.parent_trade_number = b.trade_number
                  where a.parent_trade_number is not null
                    and a.trade_type = 'RechargeRollback'
                    and b.trade_type = 'Recharge'
                    and a.trade_amount = b.trade_amount
                    and a.status = 'Succeed'
                    and b.status = 'Succeed'
                    and date_format(a.create_time, '%Y-%m-%d') = '${a}'
                  group by a.station_code,
                           a.operator_code,
                           date_format(a.create_time, '%Y-%m-%d'),
                           a.card_number) a
         group by a.date, a.station_code, a.card_number

         union all

-- 储值卡消费
         select a.date,
                a.station_code,
                a.operator_code,
                a.card_number,
                0                            as recharge_realmoney,
                0                            as recharge_totalmoney,
                0                            as recharge_count,
                sum(a.consume_realmoney)                  as consume_realmoney,
                sum(a.consume_totalmoney)                  as consume_totalmoney,
                sum(a.consume_count) - sum(a.consume_back_count) as consume_count
         from (
                  select station_code,
                         operator_code,
                         date_format(create_time, '%Y-%m-%d') as date,
                         card_number,
                         -sum(in_out_flag * pay_amount)       as consume_realmoney,
                         -sum(in_out_flag * trade_amount)     as consume_totalmoney,
                         0                                    as consume_count,
                         0                                    as consume_back_count
                  from cn_sisyphe_ods.askcard_db_trade_record
                  where trade_type in
                        ('CardConsume', 'CardConsumeRefund', 'Consume', 'ConsumeAdjustAdd', 'ConsumeAdjustReduce',
                         'ConsumeRefund')
                    and date_format(create_time, '%Y-%m-%d') = '${a}'
                  group by station_code,
                           operator_code,
                           date_format(create_time, '%Y-%m-%d'),
                           card_number
                  union all
                  select station_code,
                         operator_code,
                         date_format(create_time, '%Y-%m-%d') as date,
                         card_number,
                         0                                    as consume_realmoney,
                         0                                    as consume_totalmoney,
                         count(trade_number)                  as consume_count,
                         0                                    as consume_back_count
                  from cn_sisyphe_ods.askcard_db_trade_record
                  where trade_type = 'CardConsume'
                    and date_format(create_time, '%Y-%m-%d') = '${a}'
                  group by station_code,
                           operator_code,
                           date_format(create_time, '%Y-%m-%d'),
                           card_number
                  union all
                  select a.station_code,
                         a.operator_code,
                         date_format(a.create_time, '%Y-%m-%d') as date,
                         a.card_number,
                         0                                      as consume_realmoney,
                         0                                      as consume_totalmoney,
                         0                                      as consume_count,
                         count(a.trade_number)                  as consume_back_count
                  from cn_sisyphe_ods.askcard_db_trade_record a
                           inner join cn_sisyphe_ods.askcard_db_trade_record b on a.parent_trade_number = b.trade_number
                  where a.parent_trade_number is not null
                    and a.trade_type = 'CardConsumeRefund'
                    and b.trade_type = 'CardConsume'
                    and a.trade_amount = b.trade_amount
                    and date_format(a.create_time, '%Y-%m-%d') = '${a}'
                  group by a.station_code,
                           a.operator_code,
                           date_format(a.create_time, '%Y-%m-%d'),
                           a.card_number) a
         group by a.date,
                  a.station_code,
                  a.operator_code,
                  a.card_number) a
group by date,
         station_code,
         operator_code,
         card_number;</sql>
