  <step>
    <name>充值卡数与用卡消费笔数</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>logsystem_192.168.21.47</connection>
    <sql>select a.操作日期,a.站点ID,操作站点,count(*) as 数量,'充值卡数量' as 类型 from (
select str_to_date(a.`create_time`, '%Y-%m-%d') as 操作日期,a.station_code as 站点ID,substring_index(c.station_name,"_",1) as 操作站点,a.card_number as 卡号,
       case when a.in_out_flag=1 then sum(a.pay_amount) else 0 end -
       case when a.in_out_flag=-1 then sum(a.pay_amount) else 0 end
                                                as 交易实洋
from askcard_db.trade_record a left join askcard_db.card_info b on a.card_number=b.card_number
left join coffee_baseinfo_v2.station c on a.station_code=c.station_code
where
-- a.trade_type='Recharge' and a.station_code='AVAB01'and
date_format(a.`create_time`, '%Y-%m-%d') >  DATE_SUB(curdate(),INTERVAL +9 DAY)
and b.card_class='零售阅读卡' and a.status='Succeed' and c.station_name like '%书店%' 
and a.trade_type in ('Recharge','RechargeRefund','RechargeRollback','RechargeAdjustAdd','RechargeAdjustReduce')
-- 'Recharge,现金充值,RechargeRefund，充值退款,RechargeRollback，充值撤单,RechargeAdjustAdd"充值调整-增加",RechargeAdjustReduce"充值调整-减少"
group by str_to_date(a.`create_time`, '%Y-%m-%d'),a.station_code,a.card_number
)a where a.交易实洋>0
group by a.操作日期,a.站点ID


union all

select distinct str_to_date(a.`create_time`, '%Y-%m-%d') as 操作日期,a.station_code as 站点ID,substring_index(d.station_name,"_",1) as 操作站点,ifnull(b.充值次数,0)-ifnull(c.充值退款次数,0) as 数量,'用卡消费笔数量' as 类型
from askcard_db.trade_record a left join askcard_db.card_info bb on a.card_number=bb.card_number and bb.card_class='零售阅读卡' and a.status='Succeed'
left join
(
select str_to_date(a.`create_time`, '%Y-%m-%d') as 充值时间,count(*) as 充值次数,a.station_code
from askcard_db.trade_record a
where trade_type='CardConsume'
group by str_to_date(a.`create_time`, '%Y-%m-%d'),a.station_code
) b on str_to_date(a.`create_time`, '%Y-%m-%d') =b.充值时间 and a.station_code=b.station_code
left join
(
select str_to_date(a.`create_time`, '%Y-%m-%d') as 充值时间,count(*)*2 as 充值退款次数,a.station_code
from askcard_db.trade_record a
where trade_type='CardConsumeRefund'
group by str_to_date(a.`create_time`, '%Y-%m-%d'),a.station_code
)c on str_to_date(a.`create_time`, '%Y-%m-%d') =c.充值时间 and a.station_code=c.station_code
left join
coffee_baseinfo_v2.station d
on a.station_code=d.station_code
where date_format(a.`create_time`, '%Y-%m-%d') >  DATE_SUB(curdate(),INTERVAL +9 DAY)</sql>
