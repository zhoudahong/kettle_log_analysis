    <connection>logsystem_192.168.21.47</connection>
    <sql>
select  date_format(a.create_time, '%Y-%m-%d') as 办理时间,
'书店' AS `业务类型`,
count(*) `新增会员数(书店)`,
a.station_code as 站点ID,substring_index(b.station_name,"_",1) as 操作站点
from member_database.member_info a
left join
coffee_baseinfo_v2.station b
on a.station_code=b.station_code
left join
member_database.member_behavior_log c
