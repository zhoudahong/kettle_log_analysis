    <connection>logsystem_192.168.21.47</connection>
    <sql>-- 贵宾卡消费实洋
select str_to_date(`t`.`create_time`, '%Y-%m-%d')  AS `操作时间`,
                                         substring_index(`d`.`station_name`,"_",1)                          AS `操作站点`,
                                          d.station_code as 站点ID,
                                         '书店'                                        AS `业务类型`,
                                         ( sum((case
                                                 when (`t`.`in_out_flag` = -(1))
                                                         then (case when isnull(`t`.`pay_amount`) then 0 else `t`.`pay_amount` end)
                                                 else 0 end)) - sum((case
                                                                       when (`t`.`in_out_flag` = 1)
                                                                               then (case when isnull(`t`.`pay_amount`) then 0 else `t`.`pay_amount` end)
                                                                       else 0 end))) AS `实洋`
                                  from (((`askcard_db`.`trade_record` `t` left join `askcard_db`.`card_info` `c` on ((
                                    `t`.`card_id` = `c`.`card_id`))) left join `coffee_baseinfo_v2`.`station` `d` on ((
                                    `d`.`station_code` =
                                    `t`.`station_code`))) left join `oauth_database`.`user` `e` on ((`t`.`operator_code`
                                                                                                     =
                                                                                                     `e`.`user_code`)))
                                  where ((`t`.`status` = 'Succeed') and (`d`.`station_name` like '%书店%') and
                                         (`t`.`trade_type` in ('CardConsume', 'CardConsumeRefund','ConsumeRollback', 'ConsumeRefund','ConsumeAdjustAdd','ConsumeAdjustReduce'))
                                          and date_format(`t`.`create_time`, '%Y-%m-%d') >  DATE_SUB(curdate(),INTERVAL +9 DAY)
                                              and c.card_class in ('零售阅读卡','团购卡','员工福利','公关赠送')
                                            )
                                           -- CardConsume 划卡消费，ConsumeRefund 划卡消费退款，ConsumeRollback 消费撤单，ConsumeRefund 消费退款，ConsumeAdjustAdd 消费调整-增加，ConsumeAdjustReduce 消费调整-减少
                                  group by str_to_date(`t`.`create_time`, '%Y-%m-%d'), `d`.`station_name`,d.station_code</sql>
