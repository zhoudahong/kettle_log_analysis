    <sql>
select a.﻿flag_batch,a.up_date,a.year,a.fina_year_month,a.new_station_code,
       a.project_name1,a.num_station1,a.total_score1,a.result_points1,
       b.project_name2,b.num_station2,b.total_score2,b.result_points2,
       c.project_name3,c.num_station3,c.total_score3,c.result_points3
from 
(select temp1.﻿flag_batch,temp1.second_project_level_name as project_name1,temp1.up_date,temp1.year,temp1.fina_year_month,count(DISTINCT temp1.new_station_code) as num_station1,
temp1.new_station_code,sum(temp1.score) as total_score1,sum(temp1.result_points) as result_points1
FROM sisyphe_coff_teach_check_standard_data_modify temp1
GROUP BY temp1.﻿flag_batch,temp1.second_project_level_name,temp1.up_date,temp1.year,temp1.fina_year_month,temp1.new_station_code
) a
LEFT JOIN
(select temp2.﻿flag_batch,temp2.second_project_level_name as project_name2,temp2.up_date,temp2.year,temp2.fina_year_month,count(DISTINCT temp2.new_station_code) as num_station2,
temp2.new_station_code,sum(temp2.score) as total_score2,sum(temp2.result_points) as result_points2
from sisyphe_coff_teach_employee_management_data_modify temp2
GROUP BY temp2.﻿flag_batch,temp2.second_project_level_name,temp2.up_date,temp2.year,temp2.fina_year_month,temp2.new_station_code
)b 
on a.﻿flag_batch=b.﻿flag_batch and a.up_date=b.up_date and a.year=b.year and a.fina_year_month=b.fina_year_month and a.new_station_code=b.new_station_code
LEFT JOIN
(SELECT temp3.flag_batch,temp3.second_project_level_name as project_name3,temp3.up_date,temp3.year,temp3.fina_year_month,count(DISTINCT temp3.new_station_code) as num_station3,
temp3.new_station_code,sum(temp3.total_execution_result) as total_score3,sum(temp3.index_execution_result) as result_points3
from sisyphe_coff.sisyphe_coff_teach_execution_data_modify temp3
GROUP BY temp3.flag_batch,temp3.second_project_level_name,temp3.up_date,temp3.year,temp3.fina_year_month,temp3.new_station_code
) c
on a.﻿flag_batch=c.flag_batch and a.up_date=c.up_date and a.year=c.year and a.fina_year_month=c.fina_year_month and a.new_station_code=c.new_station_code
</sql>
