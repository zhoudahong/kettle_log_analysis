    </partitioning>
    <connection>192.168.21.44_cn_sisyphe_dwd</connection>
    <sql>select a.*, ifnull(b.full_op, '未完整经营') as full_op
from (
         select fina_year_month,
                a.station_code,
                case
                    when year(a.date) = year(start_time) then '新店'
                    when year(a.date) = year(start_time) + 1 then '次新店'
                    when year(a.date) > year(start_time) + 1 then '老店' end as station_new_old,
                case
                    when date_add(fina_month_start_date, interval -1 year) >= start_time then '可同比'
                    else '不可同比' end                                        as station_sply,
                category_two_code,
                product_new_old,
                sum(sale_total_money)                                      as sale_total_money,
                sum(delivery_total_money)                                  as delivery_total_money,
                sum(restock_total_money)                                   as restock_total_money,
                sum(stock_total_money)                                     as stock_total_money
         from (
                  select date(a.create_time) as date,
                         from_location_code  as station_code,
                         category_two_code,
                         case
                             when year(a.create_time) = year(b.create_time) then '新品'
                             else '老品' end   as product_new_old,
                         sum(amount *
                             (case bill_purpose when 'OUT_STORAGE' then 1 when 'IN_STORAGE' then -1 end) *
                             product_price)  as sale_total_money,
                         null                as delivery_total_money,
                         null                as restock_total_money,
                         null                as stock_total_money
                  from cn_sisyphe_ods.ods_book_offset_offset_inc a
                           inner join cn_sisyphe_dds.product b on a.goods_code = b.product_code
                  where bill_type in ('SALE', 'BATCH_SALE')
                    and from_location_type = 'BOOKSTORE'
                  group by date(a.create_time), from_location_code, category_two_code,
                           case when year('2019-12-31') = year(b.create_time) then '新品' else '老品' end -- 销售
                  union all
                  select date(a.create_time) as date,
                         from_location_code  as station_code,
                         category_two_code,
                         case
                             when year(a.create_time) = year(b.create_time) then '新品'
                             else '老品' end   as product_new_old,
                         null                as sale_total_money,
                         sum(amount *
                             (case bill_purpose when 'OUT_STORAGE' then -1 when 'IN_STORAGE' then 1 end) *
                             product_price)  as delivery_total_money,
                         null                as restock_total_money,
                         null                as stock_total_money
                  from (
                           select *
                           from cn_sisyphe_ods.ods_book_offset_offset_inc
                           where bill_type = 'DELIVERY'
                           union all
                           select a.*
                           from (select *
                                 from cn_sisyphe_ods.ods_book_offset_offset_inc
                                 where bill_type = 'MISTAKE') a
                                    inner join (select bill_code
                                                from cn_sisyphe_ods.ods_book_offset_offset_inc
                                                where bill_type = 'DELIVERY') b
                                               on a.related_bill_code = b.bill_code) a
                           inner join cn_sisyphe_dds.product b on a.goods_code = b.product_code
                  where from_location_type = 'BOOKSTORE'
                  group by date(a.create_time), from_location_code, category_two_code,
                           case
                               when year(a.create_time) = year(b.create_time) then '新品'
                               else '老品' end -- 配入
                  union all
                  select date(a.create_time) as date,
                         from_location_code  as station_code,
                         category_two_code,
                         case
                             when year(a.create_time) = year(b.create_time) then '新品'
                             else '老品' end   as product_new_old,
                         null                as sale_total_money,
                         null                as delivery_total_money,
                         sum(amount *
                             (case bill_purpose when 'OUT_STORAGE' then 1 when 'IN_STORAGE' then -1 end) *
                             product_price)  as restock_total_money,
                         null                as stock_total_money
                  from (
                           select *
                           from cn_sisyphe_ods.ods_book_offset_offset_inc
                           where bill_type = 'RESTOCK'
                           union all
                           select a.*
                           from (select *
                                 from cn_sisyphe_ods.ods_book_offset_offset_inc
                                 where bill_type = 'MISTAKE') a
                                    inner join (select bill_code
                                                from cn_sisyphe_ods.ods_book_offset_offset_inc
                                                where bill_type = 'RESTOCK') b
                                               on a.related_bill_code = b.bill_code) a
                           inner join cn_sisyphe_dds.product b on a.goods_code = b.product_code
                  where from_location_type = 'BOOKSTORE'
                  group by date(a.create_time), from_location_code, goods_code -- 退库
                  union all
                  select date,
                         station_code,
                         category_two_code,
                         product_new_old,
                         null as sale_total_money,
                         null as delivery_total_money,
                         null as restock_total_money,
                         stock_total_money
                  from cn_sisyphe_ods.stock_for_comm_mana_monthly
              ) a
                  inner join sisyphe_dds.date_view b on a.date = b.date
                  inner join cn_sisyphe_ods.station_start_date c on a.station_code = c.station_code
         group by fina_year_month,
                  a.station_code,
                  case
                      when year(a.date) = year(start_time) then '新店'
                      when year(a.date) = year(start_time) + 1 then '次新店'
                      when year(a.date) > year(start_time) + 1 then '老店' end,
                  case
                      when date_add(fina_month_start_date, interval -1 year) >= start_time then '可同比'
                      else '不可同比' end,
                  category_two_code,
                  product_new_old) a
         left join
     cn_sisyphe_ods.bookstore_full_op b
     on a.fina_year_month = b.fina_year_month and a.station_code = b.station_code
where a.fina_year_month=(select 财务月 from  sisyphe_dds.自然财务月);</sql>
