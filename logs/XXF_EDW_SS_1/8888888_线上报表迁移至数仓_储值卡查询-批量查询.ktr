    <connection>192.168.21.44_sisyphe_ods</connection>
    <sql>select a.trade_number                                                                          as 交易单号,
      date_format(a.create_time, '%Y-%m-%d %H:%i:%S')                                          as 操作时间,
       a.card_number                                                                           as 卡号,
       case when a.in_out_flag=1 and f.操作类型编码='ExchangeCard' then '换卡转入'
            when a.in_out_flag=-1 and f.操作类型编码='ExchangeCard' then '换卡转出'
            when a.in_out_flag=-1 and f.操作类型编码='RepeatCardRecharge' then '重复卡办理转入'
            when a.in_out_flag=-1 and f.操作类型编码='RepeatCardRecharge' then '重复卡办理转出'
          else  f.操作类型名称 end  as 操作类型,
       b.station_name                                                                          as 操作站点,
       case when in_out_flag = 1 then a.trade_amount else 0 end                                as 充值码洋,
       case when in_out_flag = 1 then a.pay_amount else 0 end                                  as 充值实洋,
       case when in_out_flag = 1 then a.discount else 0 end                                    as 充值折扣,
       case when in_out_flag = -1 then a.pay_amount else 0 end                                 as 消费实洋,
       sum(e.offset_consume_amount * e.offset_consume_discount)                                as 成本金额,
       sum(e.offset_consume_amount * e.offset_consume_discount) / sum(e.offset_consume_amount) as 成本折扣,
       a.balance                                                                               as 余额,
       case
         when d.status = 'Unactivated' then '未激活'
         when d.status = 'Locked' then '冻结'
         when d.status = 'Available' then '正常'
         when d.status = 'Lost' then '挂失'
         when d.status = 'Destroyed' then '销毁'
         when d.status = 'Abnormal' then '异常'
         else d.status end                                                                     as 卡状态,
       d.card_class                                                                            as 卡类型,
       case when d.password != null then '已设密码' else '未设密码' end                                   密码状态,
       u.user_name                                                                             as 操作人,
       a.memo                                                                                  as 备注,
       a.source_id                                                                             as 原单号

       from (select * from sisyphe_ods.askcard_db_trade_record where  create_time > ? and create_time&lt;str_to_date(DATE_FORMAT(NOW(),'%Y-%m-%d'),'%Y-%m-%d %H:%i:%s') and status='Succeed')a
       inner join sisyphe_ods.coffee_baseinfo_v2_station b on a.station_code = b.station_code
       inner join sisyphe_ods.askcard_db_card_info d on a.card_number = d.card_number
       inner join sisyphe_ods.oauth_database_user u on a.operator_code = u.user_code
       left join sisyphe_ods.askcard_db_offset_consume e on a.trade_number = e.trade_number
       left join sisyphe_dds.操作类型_view f on a.trade_type=f.操作类型编码
group by a.card_number,a.trade_number

