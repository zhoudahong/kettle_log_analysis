    <name>断缺货判断基础数据_正常单</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>192.168.21.44_sisyphe_ods</connection>
    <sql>-- 补数据
select '${a}'           as date,
       a.*,
       b.sale_station_count,
       d.exist_station_count,
       new_station_name as station_name,
       station_region_name,
       logistics_name,
       category_name    as station_type,
       e.product_name,
       e.product_isbn,
       e.product_price,
       e.category_two_name,
       f.supplier_name
from (
         select a.*, c.stock_amount_logis
         from (
                  select a.station_code,
                         a.supplier_code,
                         a.product_code,
                         sum(sale_amount_60d)             as sale_amount_60d,
                         sum(sale_amount_30d)             as sale_amount_30d,
                         sum(sale_amount_15d)             as sale_amount_15d,
                         sum(sale_amount_7d)              as sale_amount_7d,
                         sum(stock_amount_normal)         as stock_amount_normal,
                         sum(stock_amount_normal_storage) as stock_amount_normal_storage
                  from (
-- 近n天零售数
                           select from_location_code                                                               as station_code,
                                  supplier_code,
                                  a.goods_code                                                                     as product_code,
                                  sum(a.amount *
                                      (case bill_purpose when 'OUT_STORAGE' then 1 when 'IN_STORAGE' then -1 end)) as sale_amount_60d,
                                  null                                                                             as sale_amount_30d,
                                  null                                                                             as sale_amount_15d,
                                  null                                                                             as sale_amount_7d,
                                  null                                                                             as stock_amount_normal,
                                  null                                                                             as stock_amount_normal_storage,
                                  null                                                                             as stock_amount_logis
                           from cn_sisyphe_dwd.dwd_book_offset_offset_inc a
                                    inner join cn_sisyphe_dwd.dwd_book_offset_batch_inc b
                                               on a.goods_code = b.goods_code and a.batch_no = b.batch_no
                                    inner join (select sale_order_code
                                                from cn_sisyphe_dwd.dwd_book_sale_v2_sale_order
                                                where create_time between date_add('${a}', interval -59 day) and date_add('${a}', interval 1 day)
                                                  and sale_type = 'SALE') c
                                               on a.bill_code = c.sale_order_code
                           group by from_location_code, supplier_code, a.goods_code
                           union all
                           select from_location_code                                                               as station_code,
                                  supplier_code,
                                  a.goods_code                                                                     as product_code,
                                  null                                                                             as sale_amount_60d,
                                  sum(a.amount *
                                      (case bill_purpose when 'OUT_STORAGE' then 1 when 'IN_STORAGE' then -1 end)) as sale_amount_30d,
                                  null                                                                             as sale_amount_15d,
                                  null                                                                             as sale_amount_7d,
                                  null                                                                             as stock_amount_normal,
                                  null                                                                             as stock_amount_normal_storage,
                                  null                                                                             as stock_amount_logis
                           from cn_sisyphe_dwd.dwd_book_offset_offset_inc a
                                    inner join cn_sisyphe_dwd.dwd_book_offset_batch_inc b
                                               on a.goods_code = b.goods_code and a.batch_no = b.batch_no
                                    inner join (select sale_order_code
                                                from cn_sisyphe_dwd.dwd_book_sale_v2_sale_order
                                                where create_time between date_add('${a}', interval -59 day) and date_add('${a}', interval 1 day)
                                                  and sale_type = 'SALE') c
                                               on a.bill_code = c.sale_order_code
                           group by from_location_code, supplier_code, a.goods_code
                           union all
                           select from_location_code                                                               as station_code,
                                  supplier_code,
                                  a.goods_code                                                                     as product_code,
                                  null                                                                             as sale_amount_60d,
                                  null                                                                             as sale_amount_30d,
                                  sum(a.amount *
                                      (case bill_purpose when 'OUT_STORAGE' then 1 when 'IN_STORAGE' then -1 end)) as sale_amount_15d,
                                  null                                                                             as sale_amount_7d,
                                  null                                                                             as stock_amount_normal,
                                  null                                                                             as stock_amount_normal_storage,
                                  null                                                                             as stock_amount_logis
                           from cn_sisyphe_dwd.dwd_book_offset_offset_inc a
                                    inner join cn_sisyphe_dwd.dwd_book_offset_batch_inc b
                                               on a.goods_code = b.goods_code and a.batch_no = b.batch_no
                                    inner join (select sale_order_code
                                                from cn_sisyphe_dwd.dwd_book_sale_v2_sale_order
                                                where create_time between date_add('${a}', interval -59 day) and date_add('${a}', interval 1 day)
                                                  and sale_type = 'SALE') c
                                               on a.bill_code = c.sale_order_code
                           group by from_location_code, supplier_code, a.goods_code
                           union all
                           select from_location_code                                                               as station_code,
                                  supplier_code,
                                  a.goods_code                                                                     as product_code,
                                  null                                                                             as sale_amount_60d,
                                  null                                                                             as sale_amount_30d,
                                  null                                                                             as sale_amount_15d,
                                  sum(a.amount *
                                      (case bill_purpose when 'OUT_STORAGE' then 1 when 'IN_STORAGE' then -1 end)) as sale_amount_7d,
                                  null                                                                             as stock_amount_normal,
                                  null                                                                             as stock_amount_normal_storage,
                                  null                                                                             as stock_amount_logis
                           from cn_sisyphe_dwd.dwd_book_offset_offset_inc a
                                    inner join cn_sisyphe_dwd.dwd_book_offset_batch_inc b
                                               on a.goods_code = b.goods_code and a.batch_no = b.batch_no
                                    inner join (select sale_order_code
                                                from cn_sisyphe_dwd.dwd_book_sale_v2_sale_order
                                                where create_time between date_add('${a}', interval -59 day) and date_add('${a}', interval 1 day)
                                                  and sale_type = 'SALE') c
                                               on a.bill_code = c.sale_order_code
                           group by from_location_code, supplier_code, a.goods_code
                           union all
-- 零售门店库存数（正常库）
                           select station_code,
                                  supplier_code,
                                  product_code,
                                  null          as sale_amount_60d,
                                  null          as sale_amount_30d,
                                  null          as sale_amount_15d,
                                  null          as sale_amount_7d,
                                  sum(a.amount) as stock_amount_normal,
                                  null          as stock_amount_normal_storage,
                                  null          as stock_amount_logis
                           from cn_sisyphe_dwd.stock_detail_daily a
                                    inner join cn_sisyphe_dds.dds_station b on a.station_code = b.new_station_code
                                    inner join cn_sisyphe_dwd.dwd_book_offset_batch_inc c
                                               on a.batch_no = c.batch_no and a.product_code = c.goods_code
                           where a.amount &lt;&gt; 0
                             and date = '${a}'
                             and storage = 'NORMAL'
                             and category_code = 'BOOKSTORE'
                           group by station_code,
                                    supplier_code,
                                    a.product_code

                           union all
-- 零售门店库存数（正常+在途库）
                           select station_code,
                                  supplier_code,
                                  product_code,
                                  null          as sale_amount_60d,
                                  null          as sale_amount_30d,
                                  null          as sale_amount_15d,
                                  null          as sale_amount_7d,
                                  null          as stock_amount_normal,
                                  sum(a.amount) as stock_amount_normal_storage,
                                  null          as stock_amount_logis
                           from cn_sisyphe_dwd.stock_detail_daily a
                                    inner join cn_sisyphe_dds.dds_station b on a.station_code = b.new_station_code
                                    inner join cn_sisyphe_dwd.dwd_book_offset_batch_inc c
                                               on a.product_code = c.goods_code and a.batch_no = c.batch_no
                           where a.amount &lt;&gt; 0
                             and date = '${a}'
                             and storage in ('NORMAL', 'ON_STORAGE')
                             and category_code = 'BOOKSTORE'
                           group by station_code,
                                    supplier_code,
                                    a.product_code

                           union all
-- 物流库存数
                           select station_code,
                                  supplier_code,
                                  product_code,
                                  null          as sale_amount_60d,
                                  null          as sale_amount_30d,
                                  null          as sale_amount_15d,
                                  null          as sale_amount_7d,
                                  null          as stock_amount_normal,
                                  null          as stock_amount_normal_storage,
                                  sum(a.amount) as stock_amount_logis
                           from cn_sisyphe_dwd.stock_detail_daily a
                                    inner join cn_sisyphe_dds.dds_station b on a.station_code = b.new_station_code
                                    inner join cn_sisyphe_dwd.dwd_book_offset_batch_inc c
                                               on a.product_code = c.goods_code and a.batch_no = c.batch_no
                           where a.amount &lt;&gt; 0
                             and date = '${a}'
                             and storage in ('NORMAL', 'STORAGE')
                             and category_code = 'LOGISTICS'
                           group by station_code,
                                    supplier_code,
                                    product_code
                       ) a
                  group by station_code,
                           supplier_code,
                           product_code
              ) a
                  inner join sisyphe_dds.dds_station b on a.station_code = b.new_station_code
                  left join
              (select station_code,
                      supplier_code,
                      product_code,
                      sum(a.amount) as stock_amount_logis
               from cn_sisyphe_dwd.stock_detail_daily a
                        inner join cn_sisyphe_dds.dds_station b on a.station_code = b.new_station_code
                        inner join cn_sisyphe_dwd.dwd_book_offset_batch_inc c
                                   on a.product_code = c.goods_code and a.batch_no = c.batch_no
               where a.amount &lt;&gt; 0
                 and date = '${a}'
                 and storage in ('NORMAL', 'STORAGE')
                 and category_code = 'LOGISTICS'
               group by station_code,
                        supplier_code,
                        product_code) c
              on b.logistic_code = c.station_code and a.supplier_code = c.supplier_code and
                 a.product_code = c.product_code
         group by station_code,
                  supplier_code,
                  product_code
     ) a
         left join
     (
-- 动销店数
         select supplier_code, product_code, count(station_code) as sale_station_count
         from (
                  select distinct supplier_code,
                                  a.goods_code       as product_code,
                                  from_location_code as station_code
                  from cn_sisyphe_dwd.dwd_book_offset_offset_inc a
                           inner join cn_sisyphe_dwd.dwd_book_offset_batch_inc b
                                      on a.goods_code = b.goods_code and a.batch_no = b.batch_no
                  where a.create_time between date_add('${a}', interval -29 day) and date_add('${a}', interval 1 day)
                    and bill_type = 'SALE'
                    and from_location_type = 'BOOKSTORE') a
         group by supplier_code, product_code
     ) b on a.supplier_code = b.supplier_code and a.product_code = b.product_code
         inner join sisyphe_dds.dds_station c on a.station_code = c.new_station_code
         left join
     (
-- 存店数（物流区域）
         select supplier_code,
                product_code,
                logistic_code,
                count(station_code) as exist_station_count
         from (
                  select distinct supplier_code, product_code, logistic_code, station_code
                  from (
                           select supplier_code,
                                  a.goods_code       as product_code,
                                  from_location_code as station_code
                           from cn_sisyphe_dwd.dwd_book_offset_offset_inc a
                                    inner join cn_sisyphe_dwd.dwd_book_offset_batch_inc b
                                               on a.goods_code = b.goods_code and a.batch_no = b.batch_no
                           where a.create_time between date_add('${a}', interval -29 day) and date_add('${a}', interval 1 day)
                             and bill_type = 'SALE'
                             and from_location_type = 'BOOKSTORE'
                           union all
                           select supplier_code,
                                  product_code,
                                  station_code
                           from cn_sisyphe_dwd.stock_detail_daily a
                                    inner join cn_sisyphe_dwd.dwd_book_offset_batch_inc b
                                               on a.product_code = b.goods_code and a.batch_no = b.batch_no
                                    inner join sisyphe_dds.dds_station c on a.station_code = c.new_station_code
                               and category_code = 'BOOKSTORE'
                           where a.amount &lt;&gt; 0
                             and date = '${a}'
                       ) a
                           inner join cn_sisyphe_dds.dds_station b on a.station_code = b.new_station_code) a
         group by supplier_code, product_code, logistic_code
     ) d on a.supplier_code = d.supplier_code and a.product_code = d.product_code and c.logistic_code = d.logistic_code
         inner join cn_sisyphe_dds.product e on a.product_code = e.product_code
         inner join (select distinct supplier_code, supplier_name from cn_sisyphe_dds.product) f
                    on a.supplier_code = f.supplier_code;</sql>
