    <connection>coff</connection>
    <sql>select create_date,staff_code,staff_name,station_code,staff_job,entry_date,train_calculation_date,complete_amount_should,complete_amount_already,
course_amount_before,course_amount_now,study_day_now,study_day_now_already,complete_amount_already/complete_amount_should as complete_rate
from
(SELECT b.create_date,a.staff_code,staff_name,a.station_code,a.staff_job,a.entry_date,a.train_calculation_date,
case when datediff(a.create_date,train_calculation_date)>study_day_now
then
(case when course_rate_amount>course_amount_before+course_amount_now then course_rate_amount else course_amount_before+course_amount_now end)
else (datediff(a.create_date,train_calculation_date)/study_day_now*course_amount_now+course_amount_before)
end as complete_amount_should,
course_amount_before,course_amount_now,study_day_now,
datediff(a.create_date,train_calculation_date) as study_day_now_already,b.complete_amount_already
from (select * from sisyphe_coff.sisyphe_coff_teach_training_staff_course_list where create_date='${CREATE_DATE}') a
left join 
(select create_date,staff_code,count(course_name) as complete_amount_already,count(complete_rate) as course_rate_amount from sisyphe_coff.sisyphe_coff_teach_training_staff_course_score
where course_type='常规课'and complete_rate>=1 
GROUP BY create_date,staff_code) as b 
on a.create_date=b.create_date and a.staff_code=b.staff_code) t
where create_date='${CREATE_DATE}';
</sql>
