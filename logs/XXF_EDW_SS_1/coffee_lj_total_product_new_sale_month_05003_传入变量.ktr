  <step>
    <name>全量-表输入sisyphe_coff_product_hsaledetail_modify0</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>coff</connection>
    <sql>select temp1.create_time_y,temp1.final_year_month, temp1.station_code,temp1.sale_amount,temp2.sale_amount_new,
case when temp2.sale_amount_new/temp1.sale_amount>=0.15 then 100 else 
(case when temp2.sale_amount_new/temp1.sale_amount>=0.1 and temp2.sale_amount_new/temp1.sale_amount&lt;0.15 then 75 ELSE
0 end )end as points
from (
select T.create_time_y,T.final_year_month, T.station_code,sum(T.sale_amount) as sale_amount
from (
select a.create_time_y,a.final_year_month, a.station_code, a.coff_sku_code, a.coff_sku_name,b.two_product_type_name,a.sale_amount
from (
select create_time_y,final_year_month, station_code, coff_sku_code, coff_sku_name,sum(sale_amount) as sale_amount
from sisyphe_coff.sisyphe_coff_product_hsaledetail_modify0
where final_year_month='2019-08' 
GROUP BY create_time_y,final_year_month, station_code, coff_sku_code, coff_sku_name
) a
INNER JOIN (select DISTINCT product_code,two_product_type_name from sisyphe_dds.cargo_raw_material )b
on a.coff_sku_code=b.product_code
) T
where T.two_product_type_name in (select DISTINCT two_product_type_name from sisyphe_coff.sisyphe_coff_product_new where up_date in (select DISTINCT max(up_date) as up_date from sisyphe_coff.sisyphe_coff_product_new ))
GROUP BY T.create_time_y,T.final_year_month, T.station_code
) temp1
LEFT JOIN 
(
select T1.final_year_month,T1.station_code,sum(T1.sale_amount) as sale_amount_new
from
(
SELECT a.final_year_month, a.station_code,a.coff_sku_code, a.coff_sku_name, b.product_name_new,b.two_product_type_name,sale_amount
from
(select final_year_month, station_code, coff_sku_code, coff_sku_name,sum(sale_amount) as sale_amount
from sisyphe_coff.sisyphe_coff_product_hsaledetail_modify0
where final_year_month='2019-08' and coff_sku_code in (select DISTINCT product_code from sisyphe_coff.sisyphe_coff_product_new where up_date in (select DISTINCT max(up_date) as up_date from sisyphe_coff.sisyphe_coff_product_new ))
GROUP BY final_year_month, station_code, coff_sku_code, coff_sku_name) a
INNER JOIN (select DISTINCT product_code, product_name_new,two_product_type_name from sisyphe_coff.sisyphe_coff_product_new where up_date in (select DISTINCT max(up_date) as up_date from sisyphe_coff.sisyphe_coff_product_new )) b
on b.product_code=a.coff_sku_code
) T1
GROUP BY final_year_month, station_code
) temp2 
on temp1.final_year_month=temp2.final_year_month and temp1.station_code=temp2.station_code </sql>
