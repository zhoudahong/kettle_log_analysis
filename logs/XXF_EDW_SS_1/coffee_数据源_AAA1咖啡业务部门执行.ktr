    <sql>select stock_date,
       station_code,
       storage_code,
       cargo_code,
       cargo_cost_price,
       sum(cargo_stock_amount) as cargo_stock_amount,
       material_code,
       material_measurement,
       sum(material_stock_amount) as material_stock_amount,
       sum(stock_cost_money) as stock_cost_money,
       produce_date,
       expire_days,
       expire_date
from (
         select date_format(adddate(now(), -1), '%Y-%m-%d') as stock_date,
                station_code,
                storage_code,
                a.cargo_code,
                unit_cost                                   as cargo_cost_price,
                surplus_amount / measurement                as cargo_stock_amount,
                a.raw_material_code                         as material_code,
                measurement                                 as material_measurement,
                surplus_amount                              as material_stock_amount,
                surplus_amount / measurement * unit_cost    as stock_cost_money,
                date_in_produced                            as produce_date,
                effective_time                              as expire_days,
                adddate(date_in_produced, effective_time)   as expire_date
         from sisyphe_ods.coffee_stock_offset a
                  inner join sisyphe_ods.coffee_bill_purchase_bill_detail b
                             on a.batch_code = b.bill_code and a.cargo_code = b.cargo_code
                  inner join sisyphe_ods.coffee_baseinfo_v2_cargo c
                             on a.cargo_code = c.cargo_code
         where surplus_amount &lt;&gt; 0 ) tbl
group by stock_date,
         station_code,
         storage_code,
         cargo_code,
         cargo_cost_price,
         material_code,
         material_measurement,
         produce_date,
         expire_days,
         expire_date;</sql>
