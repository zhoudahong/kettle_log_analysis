    <sql>select a.支付日期,a.所属财务区域名称,a.区域名称,a.所属城市,a.站点名称,a.业务类型,a.支付方式,a.所属类别
    ,ifnull(a.支付金额,0)-ifnull(b.支付金额,0) as 支付金额 from (
select a.支付日期,a.所属财务区域名称,a.区域名称,a.所属城市,a.站点名称,a.业务类型,a.支付方式,a.所属类别,sum(a.支付金额) as 支付金额 from  cw_all_check a
where 所属类别='零售' and a.支付日期>DATE_SUB(curdate(),INTERVAL +9 DAY)
group by a.支付日期,a.所属财务区域名称,a.区域名称,a.所属城市,a.站点名称,a.业务类型,a.支付方式,a.所属类别
)a
left join
(select a.支付日期,a.所属财务区域名称,a.区域名称,a.所属城市,a.站点名称,a.业务类型,a.支付方式,a.所属类别,sum(a.支付金额) as 支付金额 from  cw_all_check a
where 所属类别='零售退款金额' and a.支付日期>DATE_SUB(curdate(),INTERVAL +9 DAY)
group by a.支付日期,a.所属财务区域名称,a.区域名称,a.所属城市,a.站点名称,a.业务类型,a.支付方式,a.所属类别
)b
on a.支付日期=b.支付日期 and a.站点名称=b.站点名称
and a.业务类型=b.业务类型 and a.支付方式=b.支付方式 and a.区域名称=b.区域名称 and ifnull(a.所属财务区域名称,0)=ifnull(b.所属财务区域名称,0) and a.所属城市=b.所属城市

union all


select a.支付日期,a.所属财务区域名称,a.区域名称,a.所属城市,a.站点名称,a.业务类型,a.支付方式,a.所属类别,sum(a.支付金额) as 支付金额 from  cw_all_check a
where 所属类别='售卡充值' and a.支付日期>DATE_SUB(curdate(),INTERVAL +9 DAY) and 支付方式 is not null
group by a.支付日期,a.所属财务区域名称,a.区域名称,a.所属城市,a.站点名称,a.业务类型,a.支付方式,a.所属类别


union all
select a.支付日期,a.所属财务区域名称,a.区域名称,a.所属城市,a.站点名称,a.业务类型,a.支付方式,所属类别,a.支付金额  from  cw_all_check a
where 所属类别  not in ('零售','零售退款金额','售卡充值','售卡充值退款') and a.支付日期>DATE_SUB(curdate(),INTERVAL +9 DAY)

