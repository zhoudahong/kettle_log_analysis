  <step>
    <name>书店零售明细查询</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>192.168.21.44_sisyphe_ods</connection>
    <sql>select a.时间                        as dates,
       a.区域编码                      as station_region_code,
       a.区域名称                      as station_region_name,
       a.新系统站点ID                   as station_code,
       a.新系统站点名称                   as station_name,
       a.单据类型                      as sale_type_name,
       a.书号                        as product_isbn,
       a.书名                        as product_name,
       a.书编码                       as product_code,
       a.定价                        as product_price,
       a.零售码洋                         sale_price,
       a.零售数                       as sale_amount,
       a.零售实洋                      as sale_real_price,
       a.单品交易笔数                    as in_out_flag_count
        ,
       a.单品交易笔数 / b.交易笔数           as purchase_ratio,
       a.单品交易笔数 / c.passenger_flow as deal_ratio
from (
         select date(a.create_time)                   时间,
                e.区域编码,
                e.区域名称,
                e.新系统站点ID,
                e.新系统站点名称,
                a.sale_type_name                   as 单据类型,
                c.product_isbn                     as 书号,
                c.product_name                     as 书名,
                c.product_code                     as 书编码,
                c.product_price                    as 定价,
                sum(b.sale_price * b.sale_amount)  as 零售码洋,
                sum(b.sale_amount * a.in_out_flag) as 零售数,
                sum(b.sale_real_price)             as 零售实洋,
                sum(a.in_out_flag)                 as 单品交易笔数
         from cn_sisyphe_dwd.dwd_book_sale_v2_sale_order as a
                  inner join cn_sisyphe_dwd.dwd_book_sale_v2_sale_order_detail as b
                             on a.sale_order_id = b.sale_order_id -- and  a.sale_order_id='1429112'
                  inner join cn_sisyphe_dds.product c on b.product_code = c.product_code
             and date(a.create_time) = '${a}'
                  inner join cn_sisyphe_dds.dds_station_view as e on a.station_code = e.新系统站点ID
         group by date(a.create_time), e.区域编码, e.新系统站点ID, a.sale_type_name,
                  c.product_isbn, c.product_name, c.product_price
    having sum(b.sale_amount * a.in_out_flag) > 0) a
         left join (

-- 门店的交易笔数

    select aa.station_code, date(aa.create_time) create_time, sum(aa.in_out_flag) 交易笔数
    from cn_sisyphe_dwd.dwd_book_sale_v2_sale_order aa
    where date(aa.create_time) = '${a}'
    group by aa.station_code, date(aa.create_time)
) b on a.新系统站点ID = b.station_code and a.时间 = b.create_time

-- 营运日志的客流

         left join cn_sisyphe_dds.station_passenger_flow as c
                   on a.新系统站点ID = c.station_code and date(a.时间) = c.create_time

/*
-- 与上面SQl唯一不同的地方是这里对购买率与成交率进行了sum聚合和后面的group分组，如果按天执行的话，这种效率会慢一分钟
select a.时间,a.区域编码,a.区域名称,a.新系统站点ID,a.新系统站点名称,a.单据类型,a.书号,a.书名,a.定价,a.零售码洋,a.零售数,a.零售实洋,a.单品交易笔数
      ,sum(a.单品交易笔数/b.交易笔数) as 购买率,sum(a.单品交易笔数/c.passenger_flow) as 成交率 from (
select date(a.create_time)时间,e.区域编码,e.区域名称,e.新系统站点ID,e.新系统站点名称,a.sale_type_name as 单据类型,
       c.product_isbn as 书号,c.product_name as 书名,c.product_price as 定价,sum(b.sale_price*b.sale_amount) as 零售码洋,
         sum(b.sale_amount*a.in_out_flag)as 零售数,
       sum(b.sale_real_price)as 零售实洋,
       sum(a.in_out_flag) as 单品交易笔数
from cn_sisyphe_dwd.dwd_book_sale_sale_order_inc as a
inner join cn_sisyphe_dwd.dwd_book_sale_sale_order_detail_inc as b
on a.sale_order_id=b.sale_order_id  -- and  a.sale_order_id='1429112'
inner join cn_sisyphe_dds.product c on b.product_code = c.product_code
and date(a.create_time)='2019-10-01'
inner join cn_sisyphe_dds.dds_station_view as e on a.station_code=e.新系统站点ID
group by date(a.create_time),e.区域编码,e.新系统站点ID,a.sale_type_name,
c.product_isbn,c.product_name,c.product_price)a
left join (

-- 门店的交易笔数

select aa.station_code,date(aa.create_time)create_time,sum(aa.in_out_flag) 交易笔数
from cn_sisyphe_dwd.dwd_book_sale_sale_order_inc aa
where date(aa.create_time)='2019-10-01' -- and aa.station_name='沈阳太原街万达广场店_书店'
group by  aa.station_code,date(aa.create_time)
)b on a.新系统站点ID=b.station_code and a.时间=b.create_time
inner join cn_sisyphe_dds.station_passenger_flow as c on a.新系统站点ID=c.station_code and date(a.时间)=c.create_time
group by  a.时间,a.区域编码,a.新系统站点ID,a.单据类型,a.书号,a.书名,a.定价
