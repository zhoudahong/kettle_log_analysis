    <connection>192.168.0.2_xxfbooks</connection>
    <sql>select a.支付日期,a.站点,a.业务类型,a.站点ID
,sum(a.[01]) as '01实洋',sum(a.[02]) as '02实洋',sum(a.[03]) as '03实洋',sum(a.[04]) as '04实洋',sum(a.[05]) as '05实洋',sum(a.[06]) as '06实洋'
,sum(a.[07]) as '07实洋',sum(a.[08]) as '08实洋',sum(a.[09]) as '09实洋',sum(a.[10]) as '10实洋',sum(a.[11]) as '11实洋',sum(a.[12]) as '12实洋'
,sum(a.[13]) as '13实洋',sum(a.[14]) as '14实洋',sum(a.[15]) as '15实洋',sum(a.[21]) as '21实洋'
,sum(a.[纯零售实洋+卡提货实洋(少儿)]) as '纯零售实洋+卡提货实洋(少儿)'
,sum(a.[纯零售实洋+卡提货实洋(不二)]) as '纯零售实洋+卡提货实洋(不二)'
,sum(a.[纯零售+卡提货实洋(类别)]) as '纯零售+卡提货实洋(类别)'
,sum(a.[纯零售+卡提货实洋(类别不含少儿)]) as '纯零售+卡提货实洋(类别不含少儿)'
from (select a.支付日期,
             a.站点,
             a.业务类型,
             a.站点ID,
             case when 商品一级分类 = '01' then 销售实洋 else 0 end as '01',
             case when 商品一级分类 = '02' then 销售实洋 else 0 end as '02',
             case when 商品一级分类 = '03' then 销售实洋 else 0 end as '03',
             case when 商品一级分类 = '04' then 销售实洋 else 0 end as '04',
             case when 商品一级分类 = '05' then 销售实洋 else 0 end as '05',
             case when 商品一级分类 = '06' then 销售实洋 else 0 end as '06',
             case when 商品一级分类 = '07' then 销售实洋 else 0 end as '07',
             case when 商品一级分类 = '08' then 销售实洋 else 0 end as '08',
             case when 商品一级分类 = '09' then 销售实洋 else 0 end as '09',
