    <sql>select '${a}'             as date,
       a.station_code,
       a.supplier_code,
       a.product_code,
       a.sale_amount_60d,
       a.sale_amount_30d,
       a.sale_amount_15d,
       a.sale_amount_7d,
       a.stock_amount_normal,
       a.stock_amount_normal_storage,
       a.stock_amount_logis,
       b.sale_station_count,
       d.exist_station_count,
       c.new_station_name as station_name,
       c.station_region_name,
       c.logistics_name,
       c.category_name    as station_type,
       e.product_name,
       e.product_isbn,
       e.product_price,
       e.category_two_name,
       f.supplier_name
from cn_sisyphe_dwd.basic_data_for_comm_mana_shortage_sale_stock_2 a
         left join cn_sisyphe_dwd.basic_data_for_comm_mana_shortage_sale_station_count b
                   on a.supplier_code = b.supplier_code and a.product_code = b.product_code
         inner join cn_sisyphe_dds.dds_station c on a.station_code = c.new_station_code
         left join cn_sisyphe_dwd.basic_data_for_comm_mana_shortage_exist_station_count d
                   on a.supplier_code = d.supplier_code and a.product_code = d.product_code and
                      c.logistic_code = d.logistic_code
         inner join cn_sisyphe_dds.product e on a.product_code = e.product_code
         left join cn_sisyphe_dds.dds_supplier f
                   on a.supplier_code = f.supplier_code;</sql>
