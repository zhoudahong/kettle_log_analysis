  <step>
    <name>获取spzt资料结果宽表</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>192.168.21.44_cn_sisyphe_ods</connection>
    <sql>SELECT
        k.sku_code AS sku_code,
        k.bar_code AS bar_code,
        k.sku_name AS sku_name,
        k.price AS price,
        k.content_type AS content_type,
        ( CASE k.content_type WHEN 1 THEN LEFT ( ct.territory_code, 2 ) ELSE LEFT ( c.category_code, 2 ) END ) AS first_classification_code,
        ( CASE k.content_type WHEN 1 THEN LEFT ( ct.territory_code, 4 ) ELSE LEFT ( c.category_code, 4 ) END ) AS second_classification_code,
        ( CASE k.content_type WHEN 1 THEN LEFT ( ct.territory_code, 6 ) ELSE LEFT ( c.category_code, 6 ) END ) AS third_classification_code,
        ( CASE k.content_type WHEN 1 THEN ct.territory_code ELSE c.category_code END ) AS fourth_classification_code,
        ( CASE k.content_type WHEN 1 THEN ct.territory_name ELSE c.category_name END ) AS classification_name,
        s.supplier_code AS supplier_code,
        s.supplier_name AS supplier_name,
        GROUP_CONCAT( CONCAT( a.author_name ) SEPARATOR ',' ) AS author_name,
        p.start_flow_location AS start_flow_location,
        p.goods_index AS goods_index,
        p.spu_status  AS spu_status,
        LEFT(p.create_time,10) AS create_time,
				b.brand_code AS brand_code,
				b.brand_name AS brand_name,
				br.brand_line_name AS brand_line_name
				
		
FROM
         cn_sisyphe_ods.ods_sisyspzt_sku k
        JOIN  cn_sisyphe_ods.ods_sisyspzt_spu p ON k.spu_code = p.spu_code
        LEFT JOIN  cn_sisyphe_ods.ods_sisyspzt_spu_author_relation sar ON p.spu_code = sar.spu_code
        JOIN  cn_sisyphe_ods.ods_sisyspzt_supplier s ON s.supplier_code = p.supplier_code
        LEFT JOIN  cn_sisyphe_ods.ods_sisyspzt_author a ON a.author_code = sar.author_code
        JOIN  cn_sisyphe_ods.ods_sisyspzt_category c ON c.category_code = p.background_category_code
        LEFT JOIN  cn_sisyphe_ods.ods_sisyspzt_content_territory ct ON ct.territory_code = p.territory_code 
				JOIN cn_sisyphe_ods.ods_sisyspzt_spu_brand_relation br on p.spu_code=br.spu_code
				LEFT JOIN  cn_sisyphe_ods.ods_sisyspzt_brand b on br.brand_code=b.brand_code
 GROUP BY
        k.sku_code;</sql>
