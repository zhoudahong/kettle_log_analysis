  <step>
    <name>书店零售主表</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>192.168.21.44_cn_sisyphe_ods</connection>
    <sql>-- center
select id                        as sale_order_id,
       sale_order_code,
       member_code,
       null                      as member_name,
       null                      as member_card_number,
       stand_code,
       station_code,
       null                      as station_name,
       total_count               as total_category_count,
       total_amount,
       total_money,
       real_money,
       charge_money,
       null                      as already_money,
       null                      as return_money,
       null                      as terminal_trade_id,
       null                      as failed_result,
       memo,
       'DONE'                    as sale_order_state,
       null                      as source_code,
       sale_order_code           as payment_order_code,
       null                      as sale_type_record_id,
       null                      as all_refund,
       operator_code             as new_operator_code,
       null                      as new_operator_name,
       cast(create_time as char) as create_time,
       cast(update_time as char) as update_time,
       version,
       case
           when memo like '签售%' then 'CHECK_SALES'
           when memo like '商场内书展%' then 'SHOP_INSIDE'
           when memo like '商场外书展%' then 'SHOP_OUTER'
           else 'BOOK_SALES' end as sale_type,
       case
           when memo like '签售%' then '签售'
           when memo like '商场内书展%' then '商场内书展'
           when memo like '商场外书展%' then '商场外书展'
           else '正常零售' end       as sale_type_name,
       sale_order_type,
       case
           when sale_order_type = 'ORDER'
               then 1
           when sale_order_type = 'BACK_ORDER'
               then -1
           else 0 end            as in_out_flag,
       case
           when member_code is null
               then '非会员'
           when member_code is not null
               then '会员'
           else '其他' end         as member_type,
       null                      as batch_code,
       null                      as source,
       change_money,
       origin_system,
       create_time               as pay_date
from cn_sisyphe_ods.data_center_sale_order a
         inner join cn_sisyphe_dds.dds_station b on a.station_code = b.new_station_code and b.bookstore_system = 'YH'
where origin_system = 'DC'
  and year(create_time) >= 2020
  and id > ?
union all
-- ERP
select id                        as sale_order_id,
       a.sale_order_code,
       member_code,
       member_name,
       member_card_number,
       stand_code,
       station_code,
       station_name,
       total_category_count,
       total_amount,
       total_money,
       total_real_money          as real_money,
       charge_money,
       already_money,
       return_money,
       terminal_trade_id,
       failed_result,
       memo,
       sale_order_state,
       source_code,
       a.sale_order_code           as payment_order_code,
       sale_type_record_id,
       all_refund,
       operator_code             as new_operator_code,
       operator_name             as new_operator_name,
       cast(create_time as char) as create_time,
       cast(update_time as char) as update_time,
       version,
       sale_type,
       case sale_type
           when 'BOOK_SALES' then '正常零售'
           when 'CHECK_SALES' then '签售'
           when 'SHOP_INSIDE' then '商场内书展'
           when 'SHOP_OUTER' then '商场外书展'
           else null end         as sale_type_name,
       sale_order_type,
       case
           when sale_order_type = 'ORDER'
               then 1
           when sale_order_type = 'BACK_ORDER'
               then -1
           else 0 end            as in_out_flag,
       case
           when member_code is null
               then '非会员'
           when member_code is not null
               then '会员'
           else '其他' end         as member_type,
       batch_code,
       source,
       change_money,
       'ERP'                     as origin_system,
       pay_date
from cn_sisyphe_ods.ods_book_sale_v2_sale_order a
         inner join (select sale_order_code, max(pay_date) as pay_date
                     from cn_sisyphe_ods.ods_book_sale_v2_sale_pay_detail
                     group by sale_order_code) b on a.sale_order_code = b.sale_order_code
    and sale_order_state = 'DONE'
    and id > ?
    and a.source &lt;&gt; 'YH';</sql>
