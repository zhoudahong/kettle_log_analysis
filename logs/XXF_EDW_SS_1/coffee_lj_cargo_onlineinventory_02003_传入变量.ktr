  <step>
    <name>表输入(cargo_dailycargoinventory)</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>192.168.21.44_sisyphe_dds</connection>
    <sql>SELECT
			t.stock_date,
			t.station_code,
			t.cargo_code,
			t.cargo_status,
			t.expire_date,
			t.maturity_days,
			t4.bussinessdays,
			t.monthlyconsumption_flag,
			t.dailyinventory_amount,
			t.dailyinventory_money,
			t.dailyusage_amount,
			t.monthlyaccumulationusage_amount,
			t.monthlyaccumulationusage_amount/t4.bussinessdays as averagemonthly_consumption,  -- 平均消耗量
			t.dailyinventory_amount/(t.monthlyaccumulationusage_amount/t4.bussinessdays) as estimated_spent_days  -- 预计消耗天数
FROM
		(
			select
						t1.stock_date,
						t1.station_code,
						t1.cargo_code,
						t1.cargo_status,
						t1.expire_date,
						t1.maturity_days,
						if(t2.monthlyaccumulationusage_amount is null ,'未耗物料','已耗物料') as monthlyconsumption_flag,
						sum(t1.stock_amount) as dailyinventory_amount,
						sum(t1.cost_money) as dailyinventory_money,
						sum(dailyusage_amount) as dailyusage_amount,
						sum(t2.monthlyaccumulationusage_amount) as monthlyaccumulationusage_amount

			 FROM 			( select stock_date,yearmonth,station_code,cargo_code,cargo_status,stock_amount,cost_money,expire_date,maturity_days from sisyphe_coff.sisyphe_coff_cargo_dailycargoinventory 
										where 	stock_date='${DATE}'	 
									) t1   -- 库存主表，每日库存

			 LEFT JOIN (
										select temp1.create_time,temp1.station_code,temp1.cargo_code,temp1.dailyusage_amount,temp1.monthlyaccumulationusage_amount
										FROM
												(
													select create_time,station_code,cargo_code,dailyusage_amount*(-1) as dailyusage_amount,monthlyaccumulationusage_amount*(-1) as monthlyaccumulationusage_amount
													from sisyphe_coff.sisyphe_coff_cargo_monthlyaccumulationcargoconsumption
													where 	create_time='${DATE}'													) temp1
								 ) t2    -- 月耗用量，当月累计耗用耗用
				ON  t1.stock_date=t2.create_time and t1.station_code=t2.station_code and t1.cargo_code=t2.cargo_code
				GROUP BY 
								t1.stock_date,
								t1.station_code,
								t1.cargo_code,
								t1.cargo_status,
								t1.expire_date,
								t1.maturity_days
		)  t

left JOIN (
						 select station_code,sales_date,bussinessdays 
						 from sisyphe_coff.sisyphe_coff_station_bussinessdays 
					
						) t4
ON t4.station_code=t.station_code and t4.sales_date=t.stock_date</sql>
