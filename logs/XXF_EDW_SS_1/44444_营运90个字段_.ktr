    <sql>select '书店' as 类别,a.站点ID,a.零售时间,a.所有笔数-b.负值笔数 as 会员交易数 from (
select CONVERT(varchar(10),ls_datetime,21) as 零售时间,station_id as 站点ID,sum(total_amount) as 所有笔数 from db_ls
where m_id like'MM%' and CONVERT(varchar(10),ls_datetime,21)>'2018-12-31'
group by CONVERT(varchar(10),ls_datetime,21) ,station_id
)a
left join
		(
select CONVERT(varchar(10),ls_datetime,21) as 零售时间,station_id as 站点ID,count(*)*2 as  负值笔数 from db_ls
where total_amount like '%-%' and m_id like'MM%' and CONVERT(varchar(10),ls_datetime,21)>'2018-12-31'
group by CONVERT(varchar(10),ls_datetime,21) ,station_id
)b on a.零售时间=b.零售时间 and a.站点ID=b.站点ID</sql>
