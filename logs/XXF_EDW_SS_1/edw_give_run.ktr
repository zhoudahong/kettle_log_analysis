    <sql>select a.date                                       as create_time,-- as 日期,
       a.present_reason                             as discount_reasons,-- as 赠送类型,
       a.bill_purpose_name,-- as 单据类型,
       a.station_code                               as from_location_code,-- as 站点编码,
       a.new_station_name                           as from_location_name,-- as 站点,
       a.station_type_name,-- as 站点类型,
       b.fina_category_code,-- as 财务分类编码,
       b.fina_category_name,-- as 财务分类,
       sum(a.amount * a.price * a.in_out_flag)      as give_real_price,-- as 赠送码洋,
       sum(a.amount * a.real_price * a.in_out_flag) as give_total_price,-- as 赠送实洋,
       sum(a.amount * a.flow_price * a.in_out_flag) as distribution_price,-- as 分销成本,
       sum(a.amount * a.cost_price * a.in_out_flag) as stock_price-- as 进货成本
from (select a.date,
             a.station_code,
             b.new_station_name,
             b.category_name,
             a.product_code,
             -a.amount * a.in_out_flag                                           as amount,
             a.price,
             a.real_price,
             a.flow_price,
             a.cost_price,
             -a.in_out_flag                                                      as in_out_flag,
             ifnull(c.present_reason, '其他赠送')                                    as present_reason,
             case a.in_out_flag when -1 then '出库' when 1 then '入库' else null end as bill_purpose_name,
             case b.category_code
                 when 'SUPPLIER' then '供应商'
                 when 'BOOKSTORE' then '书店'
                 when 'CAFE' then '咖啡馆'
                 when 'LOGISTICS' then '物流'
                 when 'STAPLE' then '大宗客户'
                 when 'WHOLESALE' then '批发事业部'
                 when 'USER' then '用户' end                                       as station_type_name
      from cn_sisyphe_dwd.stock_change_detail_daily a
               inner join cn_sisyphe_dds.dds_station b
                          on a.station_code = b.new_station_code and a.bill_type = 'PRESENT' and
                             a.date = '${a}'
               left join (select a.code, ifnull(a.describe_des, b.describe_des) as present_reason
                          from cn_sisyphe_ods.ods_book_bill_donate_bill a
                                   left join cn_sisyphe_ods.ods_book_bill_donate_bill b
                                             on a.related_bill_code = b.code) c on a.offset_bill_code = c.code) a
         inner join cn_sisyphe_dds.product b
                    on a.product_code = b.product_code
group by a.date,
         a.present_reason,
         a.bill_purpose_name,
         a.station_code,
         a.new_station_name,
         a.station_type_name,
         b.fina_category_code,
         b.fina_category_name</sql>
