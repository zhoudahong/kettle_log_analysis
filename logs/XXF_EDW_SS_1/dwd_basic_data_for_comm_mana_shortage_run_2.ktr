    <sql>select a.station_code,
       a.supplier_code,
       a.product_code,
       sum(sale_amount_60d)             as sale_amount_60d,
       sum(sale_amount_30d)             as sale_amount_30d,
       sum(sale_amount_15d)             as sale_amount_15d,
       sum(sale_amount_7d)              as sale_amount_7d,
       sum(stock_amount_normal)         as stock_amount_normal,
       sum(stock_amount_normal_storage) as stock_amount_normal_storage
from (
         select station_code,
                supplier_code,
                product_code,
                sale_amount_60d,
                sale_amount_30d,
                sale_amount_15d,
                sale_amount_7d,
                stock_amount_normal,
                stock_amount_normal_storage
         from cn_sisyphe_dwd.basic_data_for_comm_mana_shortage_sale_60d
         union all
         select station_code,
                supplier_code,
                product_code,
                sale_amount_60d,
                sale_amount_30d,
                sale_amount_15d,
                sale_amount_7d,
                stock_amount_normal,
                stock_amount_normal_storage
         from cn_sisyphe_dwd.basic_data_for_comm_mana_shortage_sale_30d
         union all
         select station_code,
                supplier_code,
                product_code,
                sale_amount_60d,
                sale_amount_30d,
                sale_amount_15d,
                sale_amount_7d,
                stock_amount_normal,
                stock_amount_normal_storage
         from cn_sisyphe_dwd.basic_data_for_comm_mana_shortage_sale_15d
         union all
         select station_code,
                supplier_code,
                product_code,
                sale_amount_60d,
                sale_amount_30d,
                sale_amount_15d,
                sale_amount_7d,
                stock_amount_normal,
                stock_amount_normal_storage
         from cn_sisyphe_dwd.basic_data_for_comm_mana_shortage_sale_07d
         union all
         select station_code,
                supplier_code,
                product_code,
                sale_amount_60d,
                sale_amount_30d,
                sale_amount_15d,
                sale_amount_7d,
                stock_amount_normal,
                stock_amount_normal_storage
         from cn_sisyphe_dwd.basic_data_for_comm_mana_shortage_stock_normal
         union all
