    <connection>192.168.21.44_sisyphe_coff</connection>
    <sql>select date_format(a.create_date,'%Y-%m-%d') as create_date,a.station_code,a.weekday_name,a.week_num,
a.final_year_month,a.final_month,a.year,a.day_name,
date_format(a.date_yoy,'%Y-%m-%d') as date_yoy,date_format(a.date_wow,'%Y-%m-%d') as date_wow,
date_format(b.start_date,'%Y-%m-%d') as start_date,b.station_type_year,b.station_type,b.seating_count,
c.budget_confirm_yesno,c.revenue,
sum(case when index_type='售卡金额' then index_type_value else 0 end) card_real_money ,
sum(case index_type when '首充张数' then index_type_value else 0 end) first_recharge_count,
sum(case when index_classify='售卡指标' and index_type>0 and index_type&lt;300 then index_type_value else 0 end) card_other,
sum(case when index_classify='售卡指标' and index_type>=300 and index_type&lt;600 then index_type_value else 0 end) card_300,
sum(case when index_classify='售卡指标' and index_type>=600 and index_type&lt;1000 then index_type_value else 0 end) card_600,
sum(case when index_classify='售卡指标' and index_type>=1000 then index_type_value else 0 end) card_1000,
sum(case when index_classify='支付方式' and index_type in (0001 ,0005,0002,0003) then index_type_value else 0 end) pure_retail,
sum(case when index_classify='支付方式' and index_type ='####' then index_type_value else 0 end) card_consumption,
sum(case when index_classify='支付方式' and index_type ='0011' then index_type_value else 0 end) market_consum_card,
sum(case when index_classify='支付方式' and index_type ='0010' then index_type_value else 0 end) credit_activity,
sum(case when index_classify='支付方式' and index_type ='0009' then index_type_value else 0 end) group_discount,
sum(case when index_classify='支付方式' and index_type ='0013' then index_type_value else 0 end) activity_discount,
sum(case when index_classify='支付方式' and index_type ='0007' then index_type_value else 0 end) electronic_ticket,
sum(case when index_classify='支付方式' and index_type ='0008' then index_type_value else 0 end) paper_ticket,
sum(case when index_classify='支付方式' and index_type ='0012' then index_type_value else 0 end) free_order,
sum(case when index_classify='支付方式' and index_type ='0015' then index_type_value else 0 end) voucher,
sum(case when index_classify='支付方式' and index_type ='0016' then index_type_value else 0 end) free_ticket,
sum(case index_type when '划卡交易笔' then index_type_value else 0 end) card_consum_transactions,
sum(case index_type when '外卖交易笔' then index_type_value else 0 end) takeout_transactions,
sum(case index_type when '外卖产品数' then index_type_value else 0 end) takeout_amount,
sum(case index_type when '外卖实洋' then index_type_value else 0 end) takeout_money,
sum(case index_type when '外卖码洋' then index_type_value else 0 end) takeout_totalmoney,
sum(case index_type when '套餐数量' then index_type_value else 0 end) set_meal_amount,
sum(case index_type when '客流' then index_type_value else 0 end) passenger_flow,
sum(case index_type when '总交易笔' then index_type_value else 0 end) total_transactions,
sum(case index_type when '总产品数量' then index_type_value else 0 end) total_amount,
sum(case index_type when '总销售实洋' then index_type_value else 0 end) total_realmoney,
sum(case index_type when '总销售码洋' then index_type_value else 0 end) total_totalmoney
from
(select create_date,station_code,weekday_name,week_num,final_year_month,final_month,year,day_name,
index_type,index_type_value,index_classify,date_yoy,date_wow
 from sisyphe_coff.sisyphe_coff_sale_index_all_sum where create_date='${CREATE_DATE}') as a
left join sisyphe_coff.sisyphe_coff_station_base_info as b  on a.station_code=b.station_code
left join sisyphe_coff.sisyphe_coff_sale_monitor_month  as c  on a.station_code=c.station_code  and a.final_year_month=c.final_year_month
group by a.create_date,a.station_code;

;</sql>
