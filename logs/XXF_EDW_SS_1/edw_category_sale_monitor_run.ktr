    <connection>192.168.21.44_cn_sisyphe_dwd</connection>
    <sql>select a.create_time, a.station_code, a.类别 as type, sum(a.real_money) as real_money, sum(task_money) as task_money
from (
         select date(a.create_time)                             as create_time,
                a.station_code,
                case
                    when c.category_two_code = '01' then '少儿'
                    when c.category_three_code = '1204' then '少儿'
                    when c.category_two_code = '13' then '不二'
                    when c.category_two_code = '21' then '台历'
                    else '其他' end                               as 类别,
                b.sale_real_price * b.sale_amount * in_out_flag as real_money,
                ''                                              as task_money
         from cn_sisyphe_dwd.dwd_book_sale_v2_sale_order as a
                  inner join cn_sisyphe_dwd.dwd_book_sale_v2_sale_order_detail as b
                             on a.sale_order_code = b.sale_order_code and date(a.create_time) = '${a}'
                  inner join cn_sisyphe_dds.product as c on b.product_code = c.product_code
         union all
         select create_time, station_code, '少儿' as type, '' as real_money, juvenile as task_money
         from cn_sisyphe_dwd.book_sale_monitor_date
         where create_time = '${a}'
         union all
         select create_time, station_code, '其他' as type, '' as real_money, other as task_money
         from cn_sisyphe_dwd.book_sale_monitor_date
         where create_time = '${a}'
         union all
         select create_time, station_code, '不二' as type, '' as real_money, up2 as task_money
         from cn_sisyphe_dwd.book_sale_monitor_date
         where create_time = '${a}'
     ) a
group by create_time, station_code, 类别</sql>
