    <connection>192.168.21.44_sisyphe_ods</connection>
    <sql>/**
     * 为了期初、期末盘存金额，需要先计算出每天的变化量
     */
SELECT
       station_code,
       cargo_code,
       raw_material_code,
       sum(total_offset_amount * in_out_storage)             AS day_Happen_total_amount,
       sum(total_offset_amount /measurement * unit_cost * in_out_storage) AS day_Happen_amount_money,
       date_format(create_time, '%Y-%m-%d')                  as create_time
FROM sisyphe_ods.coffee_stock_offset
WHERE storage_code &lt;&gt; 'ON_STORAGE' and create_time &lt; str_to_date(DATE_FORMAT(NOW(),'%Y-%m-%d'),'%Y-%m-%d %H:%i:%s')
GROUP BY date_format(create_time, '%Y-%m-%d'), station_code, cargo_code, raw_material_code
</sql>
