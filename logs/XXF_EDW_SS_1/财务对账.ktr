    <connection>192.168.21.44_mysql</connection>
    <sql>select `支付日期`,
       `站点名称`,
       `所属财务区域名称`,
       `区域名称`,
       `所属城市`,
       `支付方式`,
       sum(`支付金额`)支付金额,
       `所属类别`,
       `业务类型`,
       `站点ID` from (
                       select str_to_date(`a`.`create_time`, '%Y-%m-%d') AS `支付日期`,
                              `b`.`new_station_name`                     AS `站点名称`,
                              `large_area_name`                          AS `所属财务区域名称`,
                              `station_region_name`                      AS `区域名称`,
                              `city_name`                                AS `所属城市`,
                              d.pay_method_name_unique                   AS `支付方式`,
                              sum(a.in_out_flag * `c`.`pay_amount`)      AS `支付金额`,
                              '售卡充值'                                     AS `所属类别`,
                              '咖啡'                                       AS `业务类型`,
                              `b`.`new_station_code`                     AS `站点ID`
                       from sisyphe_ods.`askcard_db_trade_record` `a`
                                join cn_sisyphe_dds.dds_station `b` on
                               `a`.`station_code` =
                               `b`.`new_station_code`
                                join sisyphe_ods.`askcard_db_pay_detail` `c` on `a`.`trade_id` =
                                                                                `c`.`trade_id`
                                left join cn_sisyphe_dds.pay_method_extend as d on c.pay_method_id = d.pay_method_code
                           and d.from_system='askcard'
                       where ((`a`.`trade_type` in
                               ('Recharge', 'RechargeAdjustAdd', 'RechargeAdjustReduce', 'RechargeRefund', 'RechargeRollback')) and
                              (`b`.`category_code` not in('BOOKSTORE','HQ'))

                           and str_to_date(`a`.`create_time`, '%Y-%m-%d') > DATE_SUB(curdate(), INTERVAL +9 DAY)
                           )
                         and a.status = 'Succeed'
                       group by str_to_date(`a`.`create_time`, '%Y-%m-%d'), `b`.`new_station_name`, d.pay_method_name_unique
                       union all
                       select str_to_date(`a`.`create_time`, '%Y-%m-%d') AS `支付日期`,
                              `b`.`new_station_name`                     AS `站点`,
                              `large_area_name`                          AS `所属财务区域名称`,
                              `station_region_name`                      AS `区域名称`,
                              `city_name`                                AS `城市名称`,
                              f.pay_method_name_unique                   AS `支付方式`,
                              sum(`c`.`pay_amount`*a.in_out_flag)                      AS `支付金额`,
                              '售卡充值退款'                                   AS `所属类别`,
                              '咖啡'                                       AS `业务类型`,
                              `b`.`new_station_code`                     AS `站点ID`
                       from sisyphe_ods.`askcard_db_trade_record` `a`
                                join cn_sisyphe_dds.dds_station `b` on
                               `a`.`station_code` =
                               `b`.`new_station_code`
                                join sisyphe_ods.`askcard_db_pay_detail` `c` on `a`.`trade_id` =
                                                                                `c`.`trade_id`
                                join
                            sisyphe_ods.`coffee_sale_pay_method` `d` on
                                    `c`.`pay_method_id` = `d`.`pay_method_code`
                                left join cn_sisyphe_dds.pay_method_extend as f on d.pay_method_code = f.pay_method_code
                           and f.from_system='askcard'
                       where ((`a`.`trade_type` in
                               ('RechargeRefund', 'RechargeAdjustReduce', 'RechargeRollback')) and
                              (`b`.`category_code` not in('BOOKSTORE','HQ')))
                         and str_to_date(`a`.`create_time`, '%Y-%m-%d') > DATE_SUB(curdate(), INTERVAL +9 DAY)
                         and a.status = 'Succeed'
                       group by str_to_date(`a`.`create_time`, '%Y-%m-%d'), `b`.`new_station_name`, f.pay_method_name_unique
                       union all
                       select str_to_date(`b`.`create_time`, '%Y-%m-%d') AS create_time,
                              `c`.`new_station_name`                     AS `station_name`,
                              `large_area_name`                          AS `所属财务区域名称`,
                              `station_region_name`                      AS `区域名称`,
                              `city_name`                                AS `城市名称`,
                              e.pay_method_name_unique                   AS `pay_method_name`,
                              sum(`a`.`pay_money` * (case
                                                         when `b`.`sale_order_type` = 'Order' then 1
                                                         when `b`.`sale_order_type` = 'BackOrder' then -1
                                                         else 0 end))    AS `金额`,
                              '零售'                                       AS `所属类别`,
                              '咖啡'                                       AS `业务类型`,
                              `c`.`new_station_code`                     AS `站点ID`
                       from sisyphe_ods.coffee_sale_sale_pay_detail `a`
                                join sisyphe_ods.`coffee_sale_sale_order` `b` on
                               `a`.`sale_order_sale_order_code` =
                               `b`.`sale_order_code`
                                join cn_sisyphe_dds.dds_station `c` on
                               `b`.`station_code` = `c`.`new_station_code`
                                join sisyphe_ods.coffee_sale_pay_method d on a.pay_method_code = d.pay_method_code
                                left join cn_sisyphe_dds.pay_method_extend as e on d.pay_method_code = e.pay_method_code
                           and e.from_system='coffee_erp'
                       where `c`.`category_code` not in('BOOKSTORE','HQ')
                         and str_to_date(`b`.`create_time`, '%Y-%m-%d') >DATE_SUB(curdate(), INTERVAL +9 DAY)
                         -- and new_station_name = '上海七宝万科店_咖啡馆'
                       group by str_to_date(`b`.`create_time`, '%Y-%m-%d'),
                                e.pay_method_name_unique, `c`.`new_station_code`
                       union all


                       select str_to_date(`a`.`create_time`, '%Y-%m-%d') AS `时间`,
                              `c`.`new_station_name`                     AS `station_name`,
                              `large_area_name`                          AS `所属财务区域名称`,
                              `station_region_name`                      AS `区域名称`,
                              `city_name`                                AS `城市名称`,
                              d.pay_method_name_unique                   AS `支付方式`,
                              sum(`b`.`pay_amount`*a.in_out_flag)                      AS `金额`,
                              '会员卡'                                      AS `所属类型`,
                              '咖啡'                                       AS `业务类型`,
                              `c`.`new_station_code`                     AS `station_code`
                       from sisyphe_ods.`member_database_trade_record` `a`
                                left join sisyphe_ods.`member_database_pay_detail` `b` on
                               `a`.`trade_id` =
                               `b`.`trade_id`
                                join cn_sisyphe_dds.dds_station `c` on
                               `a`.`station_code` = `c`.`new_station_code`
                                left join cn_sisyphe_dds.pay_method_extend as d on b.pay_method_id = d.pay_method_code
                           and d.from_system='askcard'
                       where `a`.`trade_type` in ('UpdateCard', 'NewRegister', 'Replacement') and a.status = 'Succeed'
                         and `c`.`category_code` not in('BOOKSTORE','HQ')
                         and str_to_date(`a`.`create_time`, '%Y-%m-%d') >
                             DATE_SUB(curdate(), INTERVAL +9 DAY)
                       group by str_to_date(`a`.`create_time`, '%Y-%m-%d'),
                                d.pay_method_name_unique,
                                `c`.`new_station_code`


                       union all
                       select str_to_date(a.create_time, '%Y-%m-%d') 支付日期,
                              c.new_station_name    as               站点名称,
                              `large_area_name`     AS               `所属财务区域名称`,
                              `station_region_name` AS               `区域名称`,
                              `city_name`           AS               `城市名称`,
                              d.pay_method_name_unique       as               支付方式,
                              sum(b.pay_amount*a.in_out_flag)     as               支付金额,
                              '会员卡'                 AS               `所属类别`,
                              '书店'                  AS               `业务类型`,
                              c.new_station_code    as               站点编码
                       from sisyphe_ods.member_database_trade_record as a
                                inner join sisyphe_ods.member_database_pay_detail as b on a.trade_id = b.trade_id
                                inner join cn_sisyphe_dds.dds_station as c on a.station_code = c.new_station_code
                                left join cn_sisyphe_dds.pay_method_extend as d on b.pay_method_id=d.pay_method_code
                           and d.from_system='askcard'
                       where date(a.create_time) > DATE_SUB(curdate(), INTERVAL +9 DAY)
                         and a.status = 'SUCCEED'
                         and `c`.`category_code` != 'CAFE'
                         -- and b.pay_method_name = '微信支付'
                         -- and c.station_name like '%鞍山万象汇店_书店%'
                       group by c.new_station_code, d.pay_method_name_unique, str_to_date(a.create_time, '%Y-%m-%d')

                       union all
                       select str_to_date(`a`.`create_time`, '%Y-%m-%d') AS `支付日期`,
                              `b`.`new_station_name`                     AS `站点`,
                              `large_area_name`                          AS `所属财务区域名称`,
                              `station_region_name`                      AS `区域名称`,
                              `city_name`                                AS `城市名称`,
                              d.pay_method_name_unique                   AS `支付方式`,
                              sum(`c`.`pay_amount`*a.in_out_flag)      AS `支付金额`,
                              '售卡充值'                                     AS `所属类别`,
                              '书店'                                       AS `业务类型`,
                              `b`.`new_station_code`                     AS `站点ID`
                       from sisyphe_ods.`askcard_db_trade_record` `a`
                                join cn_sisyphe_dds.dds_station `b` on
                               `a`.`station_code` =
                               `b`.`new_station_code`
                                join sisyphe_ods.`askcard_db_pay_detail` `c` on `a`.`trade_id` =
                                                                                `c`.`trade_id`
                                left join cn_sisyphe_dds.pay_method_extend as d on c.pay_method_id = d.pay_method_code
                           and d.from_system='askcard'
                       where ((`a`.`trade_type` in
                               ('Recharge', 'RechargeAdjustAdd', 'RechargeAdjustReduce', 'RechargeRefund', 'RechargeRollback')) and
                              (`b`.`category_code` != 'CAFE')
                           and str_to_date(`a`.`create_time`, '%Y-%m-%d') > DATE_SUB(curdate(), INTERVAL +9 DAY)
                           )
                         and a.status = 'Succeed'
                       group by str_to_date(`a`.`create_time`, '%Y-%m-%d'), `b`.`new_station_name`, d.pay_method_name_unique
                       union all
                       select str_to_date(`a`.`create_time`, '%Y-%m-%d') AS `支付日期`,
                              `b`.`new_station_name`                     AS `站点`,
                              `large_area_name`                          AS `所属财务区域名称`,
                              `station_region_name`                      AS `区域名称`,
                              `city_name`                                AS `城市名称`,
                              e.pay_method_name_unique                   AS `支付方式`,
                              sum(`c`.`pay_amount`*a.in_out_flag)                      AS `支付金额`,
                              '售卡充值退款'                                   AS `所属类别`,
                              '书店'                                       AS `业务类型`,
                              `b`.`new_station_code`                     AS `站点ID`
                       from sisyphe_ods.`askcard_db_trade_record` `a`
                                join cn_sisyphe_dds.dds_station `b` on
                               `a`.`station_code` =
                               `b`.`new_station_code`
                                join sisyphe_ods.`askcard_db_pay_detail` `c` on `a`.`trade_id` =
                                                                                `c`.`trade_id`
                                join
                            sisyphe_ods.`coffee_sale_pay_method` `d` on
                                    `c`.`pay_method_id` = `d`.`pay_method_code`
                                left join cn_sisyphe_dds.pay_method_extend as e on d.pay_method_code = e.pay_method_code
                           and e.from_system='askcard'
                       where ((`a`.`trade_type` in
                               ('RechargeRefund', 'RechargeAdjustReduce', 'RechargeRollback')) and
                              (`b`.`category_code` != 'CAFE') and
                              str_to_date(`a`.`create_time`, '%Y-%m-%d') > DATE_SUB(curdate(), INTERVAL +9 DAY))
                         and a.status = 'Succeed'
                       group by str_to_date(`a`.`create_time`, '%Y-%m-%d'), e.pay_method_name_unique, `b`.`new_station_code`

                       union all

                       select str_to_date(`t`.`create_time`, '%Y-%m-%d')      AS `操作时间`,
                              `d`.`new_station_name`                          AS `操作站点`,
                              `large_area_name`                               AS `所属财务区域名称`,
                              `station_region_name`                           AS `区域名称`,
                              `city_name`                                     AS `城市名称`,
                              '储值卡'                                           AS `支付方式`,
                              (sum((case
                                        when (`t`.`in_out_flag` = -(1))
                                            then (case
                                                      when isnull(`t`.`trade_amount`)
                                                          then 0
                                                      else `t`.`trade_amount` end)
                                        else 0 end)) - sum((case
                                                                when (`t`.`in_out_flag` = 1)
                                                                    then (case
                                                                              when isnull(`t`.`trade_amount`)
                                                                                  then 0
                                                                              else `t`.`trade_amount` end)
                                                                else 0 end))) AS `支付金额`,
                              '扣款金额'                                          AS `所属类别`,
                              '书店'                                            AS `业务类型`,
                              d.new_station_code                              as 站点ID

                       from (((sisyphe_ods.`askcard_db_trade_record` `t` left join sisyphe_ods.`askcard_db_card_info` `c` on ((
                               `t`.`card_id` = `c`.`card_id`))) left join cn_sisyphe_dds.dds_station `d` on ((
                               `d`.`new_station_code` =
                               `t`.`station_code`)))
                                left join sisyphe_ods.`oauth_database_user` `e` on ((`t`.`operator_code`
                           =
                                                                                     `e`.`user_code`)))
                       where `t`.`status` = 'Succeed'
                         and `d`.category_code != 'CAFE'
                         and `t`.`trade_type` in
                             ('Consume', 'CardConsume', 'CardConsumeRefund', 'ConsumeAdjustAdd', 'ConsumeRefund',
                              'ConsumeAdjustReduce')
                         and str_to_date(`t`.`create_time`, '%Y-%m-%d') > DATE_SUB(curdate(), INTERVAL +9 DAY)
                       group by str_to_date(`t`.`create_time`, '%Y-%m-%d'), `d`.`new_station_name`

                       union all

                       select str_to_date(`t`.`create_time`, '%Y-%m-%d')      AS `操作时间`,
                              `d`.`new_station_name`                          AS `操作站点`,
                              `large_area_name`                               AS `所属财务区域名称`,
                              `station_region_name`                           AS `区域名称`,
                              `city_name`                                     AS `城市名称`,
                              '储值卡'                                           AS `支付方式`,
                              (sum((case
                                        when (`t`.`in_out_flag` = -(1))
                                            then (case
                                                      when isnull(`t`.`trade_amount`)
                                                          then 0
                                                      else `t`.`trade_amount` end)
                                        else 0 end)) - sum((case
                                                                when (`t`.`in_out_flag` = 1)
                                                                    then (case
                                                                              when isnull(`t`.`trade_amount`)
                                                                                  then 0
                                                                              else `t`.`trade_amount` end)
                                                                else 0 end))) AS `支付金额`,
                              '扣款金额'                                          AS `所属类别`,
                              '咖啡'                                            AS `业务类型`,
                              d.new_station_code                              as 站点ID
                       from (((sisyphe_ods.`askcard_db_trade_record` `t` left join sisyphe_ods.`askcard_db_card_info` `c` on ((
                               `t`.`card_id` =
                               `c`.`card_id`))) left join cn_sisyphe_dds.dds_station `d` on ((
                               `d`.`new_station_code` =
                               `t`.`station_code`)))
                                left join sisyphe_ods.`oauth_database_user` `e` on ((
                               `t`.`operator_code` = `e`.`user_code`)))
                       where `t`.`status` = 'Succeed'
                         and `d`.category_code not in('BOOKSTORE','HQ')
                         and `t`.`trade_type` in
                             ('Consume', 'CardConsume', 'CardConsumeRefund', 'ConsumeAdjustAdd', 'ConsumeRefund',
                              'ConsumeAdjustReduce')
                         and str_to_date(`t`.`create_time`, '%Y-%m-%d') > DATE_SUB(curdate(), INTERVAL +9 DAY)
                       group by str_to_date(`t`.`create_time`, '%Y-%m-%d'), `d`.`new_station_name`

                       union all

                       SELECT create_time           as 支付日期
                            , d.new_station_name    as 站点名称
                            , `large_area_name`     AS `所属财务区域名称`
                            , `station_region_name` AS `区域名称`
                            , `city_name`           AS `城市名称`
                            , a.pay_method_name     as 支付方式
                            , sum(sale_money)       as 支付金额
                            , 'APP'                 as 所属类别
                            , d.category_name       as 业务类型
                            , d.new_station_code    as 站点ID
                       FROM (
                                SELECT pay_type,
                                       str_to_date(paid_time, '%Y-%m-%d') create_time,
                                       b.pay_method_name_unique           pay_method_name,
                                       cost as                            sale_money
                                FROM cn_sisyphe_ods.sisyapp_t_order as a
                                         inner join cn_sisyphe_dds.pay_method_extend as b
                                                    on a.pay_type = b.pay_method_code and b.from_system='app'
                                WHERE `status` IN (1, 2, 3)
                                  AND is_deleted = 0
                                UNION ALL
                                SELECT pay_type,
                                       str_to_date(sale.handle_time, '%Y-%m-%d')         create_time,
                                       c.pay_method_name_unique                          pay_method_name,
                                       - sale.sale_after_cost - sale_after_logis_cost as sale_money
                                FROM cn_sisyphe_ods.sisyapp_t_order_sale_after sale
                                         LEFT JOIN cn_sisyphe_ods.sisyapp_t_order b ON b.NO = sale.order_no
                                         inner join cn_sisyphe_dds.pay_method_extend as c
                                                    on b.pay_type = c.pay_method_code and c.from_system='app'
                                WHERE sale.STATUS = 2
                            ) a
                                inner join cn_sisyphe_dds.dds_station as d on d.new_station_code = 'CSNA00'
                       where create_time > DATE_SUB(curdate(), INTERVAL +9 DAY)
                       GROUP BY create_time, a.pay_method_name

                       union all
                       -- 新书店ERP6月29号上线
-- 零售
                       select date_format(a.pay_date, '%Y-%m-%d') as 支付日期,
                              c.new_station_name                     as 站点名称,
                              `large_area_name`                      AS `所属财务区域名称`,
                              `station_region_name`                  AS `区域名称`,
                              `city_name`                            AS `城市名称`,
                              d.pay_method_name_unique               as 支付方式,
                              sum(b.pay_money * a.in_out_flag)       as 支付金额,
                              '零售'                                   as 所属类别,
                              c.category_name                        as 业务类型,
                              c.new_station_code
                       from cn_sisyphe_dwd.dwd_book_sale_v2_sale_order a
                                inner join
                            cn_sisyphe_dwd.dwd_book_sale_v2_sale_pay_detail b
                            on a.sale_order_code = b.sale_order_code
                                -- and a.create_time>'2020-06-28'
                                -- and a.origin_system='ERP'
                                and new_operator_code != 'CESHI'
                                inner join
                            cn_sisyphe_dds.dds_station c
                            on a.station_code = c.new_station_code
                                left join cn_sisyphe_dds.pay_method_extend as d on b.pay_method_code = d.pay_method_code
                           and d.from_system in ('yh','book_erp')
                       where date(b.pay_date) > DATE_SUB(curdate(), INTERVAL +9 DAY)
                       group by date_format(a.pay_date, '%Y-%m-%d'), c.new_station_code, d.pay_method_name_unique

                       union all

-- 批销
                       select date(a.stock_date)                                       as create_time,
                              d.new_station_name                                        as 站点名称,
                              `large_area_name`                                         AS `所属财务区域名称`,
                              `station_region_name`                                     AS `区域名称`,
                              `city_name`                                               AS `城市名称`,
                              '批销款'                                                     as 支付方式,
                              sum(b.real_price * b.amount * (case a.bill_type
                                                                 when 'in'
                                                                     then -1
                                                                 when 'out'
                                                                     then 1
                                                                 else a.bill_type end)) as 批销实洋,
                              '批销'                                                     as 所属类别,
                              d.category_name                                           as 业务类型,
                              d.new_station_code
                       from cn_sisyphe_dwd.dwd_book_bill_sell_wholesale_bill as a
                                inner join cn_sisyphe_dwd.dwd_book_bill_sell_wholesale_bill_detail as b on a.code = b.bill_code
                                inner join cn_sisyphe_dds.dds_station d
                                           on a.own_station_code = d.new_station_code and a.own_station_code != 'CSNA00'
                       where date(a.stock_date) > DATE_SUB(curdate(), INTERVAL +9 DAY)
                       group by date(a.stock_date),
                                d.new_station_name
                   )a group by `支付日期`,
                               `支付方式`,
                               `所属类别`,
                               `业务类型`,
                               `站点ID`</sql>
