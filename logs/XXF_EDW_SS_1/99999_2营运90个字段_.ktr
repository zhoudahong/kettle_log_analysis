    <sql>
select a.类别,a.操作时间,a.区域名称,a.新系统站点名称,a.新系统站点ID,concat('全年第',WEEK(a.操作时间,1),'周') as 自然周
    ,sum(a.现金码洋)现金码洋,sum(a.现金实洋)现金实洋
    ,sum(a.银行卡码洋)银行卡码洋,sum(a.银行卡实洋)银行卡实洋,sum(a.其他纯零售码洋)其他纯零售码洋,sum(a.其他纯零售实洋)其他纯零售实洋
    ,sum(a.微信支付码洋)微信支付码洋,sum(a.微信支付实洋)微信支付实洋,sum(a.支付宝码洋)支付宝码洋,sum(a.支付宝实洋)支付宝实洋
    ,sum(a.贵宾卡码洋)贵宾卡码洋,sum(a.图礼赠券码洋)图礼赠券码洋,sum(a.图礼赠券实洋)图礼赠券实洋
    ,sum(a.批销码洋)批销码洋,sum(a.批销实洋)批销实洋,sum(a.会员交易数)会员交易数
    ,sum(a.交易数)交易数,sum(a.一级分类交易数)一级分类交易数,sum(a.销售码洋)销售码洋,sum(a.销售实洋)销售实洋
    ,sum(a.会员零售单据码洋)会员零售单据码洋,sum(a.会员零售单据实洋)会员零售单据实洋
    ,sum(a.销售册数)销售册数,sum(a.会员销售册数)会员销售册数,sum(a.少儿销售册数)少儿销售册数,sum(a.不二销售册数)不二销售册数
    ,sum(a.`销售册数(类别)`)'销售册数(类别)',sum(a.`销售册数(类别不含少儿)`)'销售册数(类别不含少儿)'
from (
select a.类别,a.操作时间,a.区域名称,a.新系统站点名称,a.新系统站点ID,a.类型
    ,case when 类型='现金' then 销售码洋 else 0 end as 现金码洋
    ,case when 类型='现金' then 销售实洋 else 0 end as 现金实洋
    ,case when 类型='银行卡' then 销售码洋 else 0 end as 银行卡码洋
    ,case when 类型='银行卡' then 销售实洋 else 0 end as 银行卡实洋
    ,case when 类型='其他纯零售' then 销售码洋 else 0 end as 其他纯零售码洋
    ,case when 类型='其他纯零售' then 销售实洋 else 0 end as 其他纯零售实洋
    ,case when 类型='微信支付' then 销售码洋 else 0 end as 微信支付码洋
    ,case when 类型='微信支付' then 销售实洋 else 0 end as 微信支付实洋
    ,case when 类型='支付宝' then 销售码洋 else 0 end as 支付宝码洋
    ,case when 类型='支付宝' then 销售实洋 else 0 end as 支付宝实洋
    ,case when 类型='贵宾卡' then 销售码洋 else 0 end as 贵宾卡码洋
    ,case when 类型='图礼赠券' then 销售码洋 else 0 end as 图礼赠券码洋
    ,case when 类型='图礼赠券' then 销售实洋 else 0 end as 图礼赠券实洋
    ,case when 类型='批销' then 销售码洋 else 0 end as 批销码洋
    ,case when 类型='批销' then 销售实洋 else 0 end as 批销实洋
    ,case when 类型='交易数' then 销售码洋 else 0 end as 交易数
    ,case when 类型='会员交易数' then 销售码洋 else 0 end as 会员交易数
    ,case when 类型='一级分类交易数' then 销售码洋 else 0 end as 一级分类交易数
    ,case when 类型='零售单据计算码洋与实洋' then 销售码洋 else 0 end as 销售码洋
    ,case when 类型='零售单据计算码洋与实洋' then 销售实洋 else 0 end as 销售实洋
    ,case when 类型='会员零售单据计算码洋与实洋' then 销售码洋 else 0 end as 会员零售单据码洋
    ,case when 类型='会员零售单据计算码洋与实洋' then 销售实洋 else 0 end as 会员零售单据实洋
    ,case when 类型='销售册数' then 销售码洋 else 0 end as 销售册数
    ,case when 类型='会员销售册数' then 销售码洋 else 0 end as 会员销售册数
    ,case when 类型='少儿销售册数' then 销售码洋 else 0 end as 少儿销售册数
    ,case when 类型='不二销售册数' then 销售码洋 else 0 end as 不二销售册数
    ,case when 类型='销售册数(类别)' then 销售码洋 else 0 end as '销售册数(类别)'
    ,case when 类型='销售册数(类别不含少儿)' then 销售码洋 else 0 end as '销售册数(类别不含少儿)'
from (
select '书店' as 类别,a.操作时间,b.区域名称,substring_index(b.新系统站点名称,"_",1) as 新系统站点名称,b.新系统站点ID,'销售' as 类型
,a.码洋 as 销售码洋,a.实洋 as 销售实洋 from(
select * from Operation_sqlserver_cq
union all
select * from operation_sqlserver_gz
)a left join 区域维度表 b on a.站点编码=b.老系统站点ID
union all
select '书店' as 类别,a.操作时间,b.区域名称,substring_index(b.新系统站点名称,"_",1) as 新系统站点名称,b.新系统站点ID,'现金' as 类型
,a.码洋 as 现金码洋,a.实洋 as 现金实洋 from(
select * from Operation_sqlserver_cq
union all
select * from operation_sqlserver_gz
)a left join 区域维度表 b on a.站点编码=b.老系统站点ID and a.支付方式='现金'
union all
select '书店' as 类别,a.操作时间,b.区域名称,substring_index(b.新系统站点名称,"_",1) as 新系统站点名称,b.新系统站点ID,'银行卡' as 类型
,a.码洋 as 银行卡码洋,a.实洋 as 银行卡实洋 from(
select * from Operation_sqlserver_cq
union all
select * from operation_sqlserver_gz
)a left join 区域维度表 b on a.站点编码=b.老系统站点ID and a.支付方式='银行卡'
union all
select '书店' as 类别,a.操作时间,b.区域名称,substring_index(b.新系统站点名称,"_",1) as 新系统站点名称,b.新系统站点ID,'其他纯零售' as 类型
,a.码洋 as 其他纯零售码洋,a.实洋 as 其他纯零售实洋
from(
select * from Operation_sqlserver_cq
union all
select * from operation_sqlserver_gz
)a left join 区域维度表 b on a.站点编码=b.老系统站点ID
where a.支付方式  in ('商场提货卡','宜居畅通卡','信用卡活动','QQ钱包')
union all
select '书店' as 类别,a.操作时间,b.区域名称,substring_index(b.新系统站点名称,"_",1) as 新系统站点名称,b.新系统站点ID,'微信支付' as 类型
,a.码洋 as 微信支付码洋,a.实洋 as 微信支付实洋
from(
select * from Operation_sqlserver_cq
union all
select * from operation_sqlserver_gz
)a left join 区域维度表 b on a.站点编码=b.老系统站点ID
where  a.支付方式 = '微信支付'
union all
select '书店' as 类别,a.操作时间,b.区域名称,substring_index(b.新系统站点名称,"_",1) as 新系统站点名称,b.新系统站点ID,'支付宝' as 类型
,a.码洋 as 支付宝码洋,a.实洋 as 支付宝实洋
from(
select * from Operation_sqlserver_cq
union all
select * from operation_sqlserver_gz
)a left join 区域维度表 b on a.站点编码=b.老系统站点ID
where a.支付方式 = '支付宝'
union all
select '书店' as 类别,a.操作时间,b.区域名称,substring_index(b.新系统站点名称,"_",1) as 新系统站点名称,b.新系统站点ID,'贵宾卡' as 类型
,a.码洋 as 贵宾卡码洋,a.实洋 as 贵宾卡实洋
from(
select * from Operation_sqlserver_cq
union all
select * from operation_sqlserver_gz
)a left join 区域维度表 b on a.站点编码=b.老系统站点ID
where  a.支付方式 in ('贵宾卡','西西弗储值卡')
union all
select '书店' as 类别,a.操作时间,b.区域名称,substring_index(b.新系统站点名称,"_",1) as 新系统站点名称,b.新系统站点ID,'图礼赠券' as 类型
,a.码洋 as 图礼赠券码洋,a.实洋 as 图礼赠券实洋
from(
select * from Operation_sqlserver_cq
union all
select * from operation_sqlserver_gz
)a left join 区域维度表 b on a.站点编码=b.老系统站点ID
where a.支付方式 in ('图礼','电子赠券','纸质赠券')
union all
select '书店' as 类别,a.操作时间,b.区域名称,substring_index(b.新系统站点名称,"_",1) as 新系统站点名称,b.新系统站点ID,类型,
a.交易数,0 as 'a' from
(
select * from cq_Transaction
union all
select * from gz_Transaction
)a left join 区域维度表 b on a.站点ID=b.老系统站点ID

union all
select '书店' as 类别,a.操作时间,b.区域名称,substring_index(b.新系统站点名称,"_",1) as 新系统站点名称,b.新系统站点ID,类型,
a.零售单据码洋,a.零售单据实洋
from (
select * from LS_Operation_sqlserver_cq
union all
select * from LS_Operation_sqlserver_gz
)a left join 区域维度表 b on a.station_id=b.老系统站点ID

union all

select '书店' as 类别,a.零售时间,b.区域名称,substring_index(b.新系统站点名称,"_",1) as 新系统站点名称,b.新系统站点ID,'销售册数' as 类型,
sum(a.销售册数) as 销售册数,0 as 'a'
from (
select * from cq_Member_Sales_volume
union all
select * from gz_Member_Sales_volume
)a left join 区域维度表 b on a.站点ID=b.老系统站点ID
group by a.零售时间,b.区域名称,substring_index(b.新系统站点名称,"_",1),b.新系统站点ID

union all

select '书店' as 类别,a.零售时间,b.区域名称,substring_index(b.新系统站点名称,"_",1) as 新系统站点名称,b.新系统站点ID,'会员销售册数' as 类型,
sum(a.销售册数) as 会员销售册数,0 as 'a'
from (
select * from cq_Member_Sales_volume
union all
select * from gz_Member_Sales_volume
)a left join 区域维度表 b on a.站点ID=b.老系统站点ID where a.m_id like 'MM%'
group by a.零售时间,b.区域名称,substring_index(b.新系统站点名称,"_",1),b.新系统站点ID

union all

select '书店' as 类别,a.零售时间,b.区域名称,substring_index(b.新系统站点名称,"_",1) as 新系统站点名称,b.新系统站点ID,'少儿销售册数' as 类型,
sum(a.销售册数) as 少儿销售册数,0 as 'a'
from (
select * from cq_Member_Sales_volume
union all
select * from gz_Member_Sales_volume
)a left join 区域维度表 b on a.站点ID=b.老系统站点ID where a.商品一级分类='01'
group by a.零售时间,b.区域名称,substring_index(b.新系统站点名称,"_",1),b.新系统站点ID

union all

select '书店' as 类别,a.零售时间,b.区域名称,substring_index(b.新系统站点名称,"_",1) as 新系统站点名称,b.新系统站点ID,'不二销售册数' as 类型,
sum(a.销售册数) as 不二销售册数,0 as 'a'
from (
select * from cq_Member_Sales_volume
union all
select * from gz_Member_Sales_volume
)a left join 区域维度表 b on a.站点ID=b.老系统站点ID where a.商品一级分类='13'
group by a.零售时间,b.区域名称,substring_index(b.新系统站点名称,"_",1),b.新系统站点ID

union all

select '书店' as 类别,a.零售时间,b.区域名称,substring_index(b.新系统站点名称,"_",1) as 新系统站点名称,b.新系统站点ID,'销售册数(类别)' as 类型,
sum(a.销售册数) as 不二销售册数,0 as 'a'
from (
select * from cq_Member_Sales_volume
union all
select * from gz_Member_Sales_volume
)a left join 区域维度表 b on a.站点ID=b.老系统站点ID where a.商品一级分类 in ('01', '02', '03', '04', '05', '06', '07', '08', '09', '10', '11', '12', '14', '15')
group by a.零售时间,b.区域名称,substring_index(b.新系统站点名称,"_",1),b.新系统站点ID

union all

select '书店' as 类别,a.零售时间,b.区域名称,substring_index(b.新系统站点名称,"_",1) as 新系统站点名称,b.新系统站点ID,'销售册数(类别不含少儿)' as 类型,
sum(a.销售册数) as 不二销售册数,0 as 'a'
from (
select * from cq_Member_Sales_volume
union all
select * from gz_Member_Sales_volume
)a left join 区域维度表 b on a.站点ID=b.老系统站点ID where a.商品一级分类 in ( '02', '03', '04', '05', '06', '07', '08', '09', '10', '11', '12', '14', '15')
group by a.零售时间,b.区域名称,substring_index(b.新系统站点名称,"_",1),b.新系统站点ID

)a where 操作时间 > DATE_SUB(curdate(),INTERVAL +9 DAY)  and (操作时间 is not null or 新系统站点名称 is not null) and 新系统站点名称 is not null
)a group by a.类别,a.操作时间,a.区域名称,a.新系统站点名称,a.新系统站点ID
</sql>
